<includes>
	<include name="VideoWallView">
		<control type="panel" id="58">
			<posx>35</posx>
			<posy>105</posy>
			<width>660</width>
			<height>376</height>
			<onleft>2</onleft>
			<onright>23</onright>
			<onup>58</onup>
			<ondown>58</ondown>
			<viewtype label="Wall">Wall</viewtype>
			<scrolltime>100</scrolltime>
			<include>list-fade</include>
			<pagecontrol>23</pagecontrol>
			<preloaditems>11</preloaditems>
			<animation effect="rotatey" start="-15" end="-15" time="0" condition="Skin.HasSetting(tilted-videowall)">Conditional</animation>
			<itemlayout height="125" width="56">
				<control type="image">
					<width>56</width>
					<height>125</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>1</posy>
					<width>55</width>
					<height>124</height>
				    <aspectratio>stretch</aspectratio>
					<colordiffuse>UnFocusedPosters</colordiffuse>
					<texture background="true" diffuse="RndVideoMask.png">DefaultVideo.png</texture>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>1</posy>
					<width>55</width>
					<height>124</height>
				    <aspectratio>stretch</aspectratio>
					<colordiffuse>UnFocusedPosters</colordiffuse>
					<texture background="true" diffuse="RndVideoMask.png">DefaultVideo.png</texture>
					<info>ListItem.Icon</info>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="138" width="84">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>58</width>
					<height>124</height>
				    <aspectratio>stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="145" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<texture background="true" diffuse="RndVideoMask.png">DefaultVideo.png</texture>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>58</width>
					<height>124</height>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="center">stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="145" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<visible>SubString(ListItem.Icon,DefaultFolderBack,Left)</visible>
				</control>
				<control type="image">
					<posx>-4</posx>
					<posy>-4</posy>
					<height>124</height>
					<width>68</width>
					<texture diffuse="RndVideoMask.png">hi-lite.png</texture>
					<aspectratio>stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="155" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<colordiffuse>IconGlow</colordiffuse>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>4</posx>
					<posy>4</posy>
					<width>50</width>
					<height>106</height>
					<texture background="true" diffuse="RndVideoMask.png">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="center">stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="155" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<visible>!SubString(ListItem.Icon,DefaultFolder,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(58)</visible>
			<!-- Markers for excess menu items -->
			<control type="image">
				<posx>352</posx>
				<posy>86</posy>
				<height>17</height>
				<width>17</width>
				<texture>scroll-up.png</texture>
				<visible>Container(58).HasPrevious</visible>
			</control>
			<control type="image">
				<posx>352</posx>
				<posy>485</posy>
				<height>17</height>
				<width>17</width>
				<texture>scroll-down.png</texture>
				<visible>Container(58).HasNext</visible>
			</control>
			<!-- MPAA Rating -->
			<control type="image">
				<posx>10</posx>
				<posy>505</posy>
				<width>120</width>
				<height>50</height>
				<texture>special://skin/extras/images/rating/mpaa_restricted.png</texture>
				<aspectratio>keep</aspectratio>
				<colordiffuse>InfoRating</colordiffuse>
				<visible>substring(listitem.mpaa,Rated R,Left)</visible>
			</control>
			<control type="image">
				<posx>10</posx>
