<includes>
	<include name="AlbumWrapView">
		<control type="wraplist" id="54">
			<viewtype label="Wrap">Wraplist</viewtype>
			<posx>0</posx>
			<posy>200</posy>
			<width>1000</width>
			<height>390</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>23</ondown>
			<include>list-fade</include>
			<focusposition>0</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>5</preloaditems>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="120" width="120">
				<control type="image">
					<posx>-24</posx>
					<posy>172</posy>
					<height>106</height>
					<width>90</width>
					<info>ListItem.Icon</info>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
				</control>
				<control type="image">
					<posx>-36</posx>
					<posy>165</posy>
					<height>120</height>
					<width>108</width>
					<texture>case-cd.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>keep</aspectratio>
					<visible>Skin.HasSetting(CDcase) + !SubString(ListItem.Icon,DefaultFolderBack,Left)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="270" width="270">
				<control type="image">
					<posx>25</posx>
					<posy>38</posy>
					<width>172</width>
					<height>240</height>
					<aspectratio>stretch</aspectratio>
					<aligny>bottom</aligny>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>22</posy>
					<height>270</height>
					<width>208</width>
					<texture>case-cd.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(CDcase) + !SubString(ListItem.Icon,DefaultFolderBack,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<!-- Scroll Bar -->
		<control type="scrollbar" id="23">
			<posx>10</posx>
			<posy>490</posy>
			<width>700</width>
			<height>22</height>
			<texturesliderbackground border="10">scrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="10">scrollbar_bar.png</texturesliderbar>
			<texturesliderbarfocus border="10">scrollbar_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<onup>54</onup>
			<ondown>2</ondown>
			<include>list-fade</include>
			<showonepage>false</showonepage>
			<orientation>horizontal</orientation>
			<colordiffuse>ScrollBarColor</colordiffuse>
			<visible>Skin.HasSetting(sbar) + Control.IsVisible(54)</visible>
		</control>
		<!-- Go Back -->
		<control type="label">
			<posx>15</posx>
			<posy>510</posy>
			<width>690</width>
			<height>30</height>
			<font>font20</font>
			<align>center</align>
			<textcolor>button-text</textcolor>
			<shadowcolor>shadow-text</shadowcolor>
			<scroll>true</scroll>
			<label>31688</label>
			<textoffsetx>0</textoffsetx>
			<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			<visible>Control.IsVisible(54)</visible>
		</control>
		<!-- Label -->
		<control type="label">
			<posx>15</posx>
			<posy>510</posy>
			<width>690</width>
			<height>30</height>
			<font>font20</font>
			<align>center</align>
			<textcolor>button-text</textcolor>
			<shadowcolor>shadow-text</shadowcolor>
			<scroll>true</scroll>
			<info>ListItem.Label</info>
			<textoffsetx>0</textoffsetx>
			<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			<visible>Control.IsVisible(54)</visible>
		</control>
		<!-- Star Rating -->
		<control type="image">
			<posx>15</posx>
			<posy>550</posy>
			<width>100</width>
			<height>20</height>
			<info>ListItem.StarRating</info>
			<colordiffuse>FlagDiffuse</colordiffuse>
			<visible>!SubString(ListItem.Icon,Default,Left)</visible>
			<visible>Control.IsVisible(54) + !IsEmpty(ListItem.StarRating)</visible>
		</control>
		<!-- Genre -->
		<control type="label">
			<posx>120</posx>
			<posy>545</posy>
			<width>480</width>
			<height>30</height>
			<font>font18</font>
			<align>center</align>
			<textcolor>button-text</textcolor>
			<shadowcolor>shadow-text</shadowcolor>
			<selectedcolor>listselectedtext</selectedcolor>
			<scroll>true</scroll>
			<info>ListItem.Genre</info>
			<textoffsetx>0</textoffsetx>
			<visible>Control.IsVisible(54)</visible>
		</control>
	</include>
</includes>