<includes>
	<include name="WrapView">
		<control type="wraplist" id="54">
			<viewtype label="31943">Wraplist</viewtype>
			<posx>20</posx>
			<posy>210</posy>
			<width>1000</width>
			<height>390</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>2</ondown>
			<include>list-fade</include>
			<focusposition>0</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>5</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="120" width="120">
				<control type="image">
					<posx>55</posx>
					<posy>165</posy>
					<width>120</width>
					<height>120</height>
					<info>ListItem.Icon</info>
					<texture background="true" diffuse="RndRectMask.png">-</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>keep</aspectratio>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>281</posy>
					<width>25</width>
					<height>25</height>
					<info>ListItem.Overlay</info>
					<colordiffuse>UnFocusedCovers</colordiffuse>
				</control>
			</itemlayout>
			<focusedlayout height="175" width="175">
				<control type="image">
					<posx>-5</posx>
					<posy>150</posy>
					<width>140</width>
					<height>140</height>
					<aspectratio>keep</aspectratio>
					<aligny>bottom</aligny>
					<info>ListItem.Icon</info>
					<texture background="true" diffuse="RndRectMask.png">-</texture>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="180" center="0,292" time="500" tween="back" easing="out" />
					</animation>
				</control>
				<control type="image">
					<posx>250</posx>
					<posy>297</posy>
					<width>60</width>
					<height>60</height>
					<info>ListItem.Overlay</info>
				</control>
			</focusedlayout>
		</control>
		<!-- Go Back -->
		<control type="label">
			<posx>10</posx>
			<posy>505</posy>
			<width>700</width>
			<height>30</height>
			<font>font28</font>
			<align>center</align>
			<textcolor>button-text</textcolor>
			<shadowcolor>shadow-text</shadowcolor>
			<scroll>true</scroll>
			<label>31688</label>
			<textoffsetx>0</textoffsetx>
			<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			<visible>Control.IsVisible(54)</visible>
		</control>
		<!-- Label -->
		<control type="label">
			<posx>10</posx>
			<posy>505</posy>
			<width>700</width>
			<height>30</height>
			<font>font28</font>
			<align>center</align>
			<textcolor>button-text</textcolor>
			<shadowcolor>shadow-text</shadowcolor>
			<scroll>true</scroll>
			<info>ListItem.Label</info>
			<textoffsetx>0</textoffsetx>
			<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			<visible>Control.IsVisible(54)</visible>
		</control>
	</include>
</includes>
