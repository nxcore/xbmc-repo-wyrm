			<!-- Go Back -->
			<control type="label">
				<posx>175</posx>
				<posy>350</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
				<animation effect="fade" delay="600" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Title -->
			<control type="label">
				<posx>175</posx>
				<posy>350</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
				<animation effect="fade" delay="600" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Genre -->
