				<height>50</height>
				<texture>special://skin/extras/images/rating/australia_r.png</texture>
				<aspectratio>keep</aspectratio>
				<visible>substring(listitem.mpaa,Australia:R)</visible>
				<visible>!IsEmpty(listitem.mpaa)</visible>
			</control>
			<control type="image">
				<posx>175</posx>
				<posy>300</posy>
				<width>120</width>
				<height>50</height>
				<texture>special://skin/extras/images/rating/australia_m.png</texture>
				<aspectratio>keep</aspectratio>
				<visible>substring(listitem.mpaa,Australia:M) + ! substring(listitem.mpaa,Australia:MA)</visible>
				<visible>!IsEmpty(listitem.mpaa)</visible>
			</control>
			<control type="image">
				<posx>175</posx>
