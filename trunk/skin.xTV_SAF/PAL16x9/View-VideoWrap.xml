<includes>
	<!-- Start Video Window Specific Views -->
	<include name="VideoWrapView">
		<control type="wraplist" id="54">
			<viewtype label="Wrap">Wraplist</viewtype>
			<posx>-28</posx>
			<posy>150</posy>
			<width>1000</width>
			<height>420</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>23</ondown>
			<include>list-fade</include>
			<focusposition>2</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>13</preloaditems>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="292" width="50">
				<control type="image">
					<posx>-36</posx>
					<posy>269</posy>
					<height>103</height>
					<width>45</width>
					<texture>dvdcase-back.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-36</posx>
					<posy>272</posy>
					<height>96</height>
					<width>44</width>
					<texture diffuse="RndVideoMask.png">defaultvideo.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-36</posx>
					<posy>272</posy>
					<height>96</height>
					<width>44</width>
					<texture fallback="defaultvideo.png" background="true" diffuse="RndVideoMask.png">$INFO[ListItem.Icon]</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-32</posx>
					<posy>272</posy>
					<height>15</height>
					<width>15</width>
					<info>ListItem.Overlay</info>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="405" width="138">
				<control type="image">
					<posx>-40</posx>
					<posy>100</posy>
					<height>284</height>
					<width>139</width>
					<texture diffuse="RndVideoMask.png">Hi-lite.png</texture>
				    <aspectratio>stretch</aspectratio>
					<colordiffuse>IconGlow</colordiffuse>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-29</posx>
					<posy>115</posy>
					<height>257</height>
					<width>117</width>
					<texture>dvdcase-back.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-30</posx>
					<posy>119</posy>
					<height>250</height>
					<width>115</width>
					<texture diffuse="RndVideoMask.png">defaultvideo.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-30</posx>
					<posy>119</posy>
					<height>250</height>
					<width>115</width>
					<texture fallback="defaultvideo.png" background="true" diffuse="RndVideoMask.png">$INFO[ListItem.Icon]</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-27</posx>
					<posy>124</posy>
					<height>39</height>
					<width>39</width>
					<info>ListItem.Overlay</info>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<animation effect="fade" delay="100" start="0" end="100" time="600">Visible</animation>
			<!-- Star Rating -->
			<control type="image">
				<posx>175</posx>
				<posy>275</posy>
				<width>100</width>
				<height>20</height>
				<info>ListItem.StarRating</info>
				<colordiffuse>StarDiffuse</colordiffuse>
				<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				<visible>!IsEmpty(ListItem.StarRating)</visible>
			</control>
			<!-- MPAA Rating -->
			<control type="image">
				<posx>175</posx>
				<posy>300</posy>
				<width>120</width>
				<height>50</height>
				<texture>special://skin/extras/images/rating/mpaa_restricted.png</texture>
				<aspectratio>keep</aspectratio>
				<colordiffuse>InfoRating</colordiffuse>
				<visible>substring(listitem.mpaa,Rated R,Left)</visible>
			</control>
			<control type="image">
				<posx>175</posx>
