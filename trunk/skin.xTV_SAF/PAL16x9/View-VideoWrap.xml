				<posy>300</posy>
				<width>120</width>
				<height>50</height>
				<texture>special://skin/extras/images/rating/bbfc_18.png</texture>
				<aspectratio>keep</aspectratio>
				<colordiffuse>InfoRating</colordiffuse>
				<visible>substring(listitem.mpaa,UK:18)</visible>
