<includes>
	<include name="WallView">
		<control type="panel" id="58">
			<posx>70</posx>
			<posy>122</posy>
			<width>625</width>
			<height>381</height>
			<onleft>2</onleft>
			<onright>23</onright>
			<onup>58</onup>
			<ondown>58</ondown>
			<viewtype label="Wall">Wall</viewtype>
			<animation effect="rotatey" start="-15" end="-15" time="0" condition="Skin.HasSetting(tilted-videowall)">Conditional</animation>
			<scrolltime>200</scrolltime>
			<include>list-fade</include>
			<pagecontrol>23</pagecontrol>
			<preloaditems>8</preloaditems>
			<itemlayout height="127" width="81">
				<control type="image">
					<width>81</width>
					<height>127</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>1</posy>
					<width>80</width>
					<height>126</height>
					<aspectratio>stretch</aspectratio>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<texture background="true" diffuse="RndMusicMask.png">DefaultProgram.png</texture>
					<info>ListItem.Icon</info>
				</control>
			</itemlayout>
			<focusedlayout height="127" width="81">
				<control type="image">
					<width>81</width>
					<height>127</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture diffuse="RndMusicMask.png">Hi-lite.png</texture>
					<aspectratio>stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="140" center="auto" time="600" tween="back" easing="out" />
						<effect type="fade" start="5" time="200" pulse="true" />
					</animation>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>1</posy>
					<width>80</width>
					<height>126</height>
					<aspectratio>stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="140" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<texture background="true" diffuse="RndMusicMask.png">DefaultProgram.png</texture>
					<info>ListItem.Icon</info>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(58)</visible>
			<!-- Title -->
			<control type="label">
				<posx>138</posx>
				<posy>500</posy>
				<width>462</width>
				<height>30</height>
				<font>font28</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>140</posx>
				<posy>522</posy>
				<width>460</width>
				<height>30</height>
				<font>font22</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
		</control>
	</include>
</includes>
