<includes>
	<include name="ListView">
		<control type="list" id="50">
			<viewtype label="31942">List</viewtype>
			<posx>400</posx>
			<posy>95</posy>
			<width>300</width>
			<height>444</height>
			<onleft>2</onleft>
			<onright>23</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<include>list-fade</include>
			<scrolltime>1</scrolltime>
			<preloaditems>5</preloaditems>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="37">
				<control type="image">
					<posx>0</posx>
					<posy>1</posy>
					<width>290</width>
					<height>30</height>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>4</posy>
					<width>24</width>
					<height>24</height>
					<texture>$INFO[ListItem.Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>-4</posy>
					<width>254</width>
					<height>37</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="37">
				<control type="image">
					<posx>22</posx>
					<posy>-4</posy>
					<width>270</width>
					<height>37</height>
					<texture>default-button.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>4</posy>
					<width>24</width>
					<height>24</height>
					<info>ListItem.Overlay</info>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>-4</posy>
					<width>254</width>
					<height>37</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<include>SortLetterNotification</include>
	</include>

	<include name="PlaylistView">
		<control type="list" id="50">
			<posx>405</posx>
			<posy>90</posy>
			<width>285</width>
			<height>444</height>
			<onleft>20</onleft>
			<onright>23</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<include>list-fade</include>
			<scrolltime>1</scrolltime>
			<viewtype label="31942">list</viewtype>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="37">
				<control type="image">
					<width>285</width>
					<height>30</height>
					<posx>0</posx>
					<posy>1</posy>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>-4</posy>
					<width>315</width>
					<height>37</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="37">
				<control type="image">
					<posx>0</posx>
					<posy>-4</posy>
					<width>285</width>
					<height>37</height>
					<texture>default-button.png</texture>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>-4</posy>
					<width>312</width>
					<height>37</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<scroll>true</scroll>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<include>SortLetterNotification</include>
	</include>

	<include name="PageControl">
		<control type="scrollbar" id="23">
			<posx>700</posx>
			<posy>90</posy>
			<width>14</width>
			<height>448</height>
			<texturesliderbackground border="0,8,0,8">scrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="0,8,0,8">scrollbar_bar.png</texturesliderbar>
			<texturesliderbarfocus border="0,8,0,8">scrollbar_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<onleft>50</onleft>
			<onright>2</onright>
			<include>list-fade</include>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<colordiffuse>ScrollBarColor</colordiffuse>
			<visible>Skin.HasSetting(sbar) + Control.IsVisible(50)</visible>
		</control>
	</include>
</includes>
