<includes>
	<include name="IconView">
		<control type="panel" id="52">
			<posx>0</posx>
			<posy>120</posy>
			<width>700</width>
			<height>394</height>
			<onleft>2</onleft>
			<onright>23</onright>
			<onup>52</onup>
			<ondown>52</ondown>
			<include>list-fade</include>
			<viewtype label="Icons">Icons</viewtype>
			<scrolltime>200</scrolltime>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="130" width="115">
				<control type="image">
					<width>100</width>
					<height>100</height>
					<posx>5</posx>
					<posy>5</posy>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>10</posy>
					<width>90</width>
					<height>90</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>75</posx>
					<posy>75</posy>
					<width>28</width>
					<height>28</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="label">
					<posx>55</posx>
					<posy>105</posy>
					<width>100</width>
					<height>22</height>
					<font>font13</font>
					<textcolor>button-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="130" width="115">
				<control type="image">
					<width>100</width>
					<height>100</height>
					<posx>5</posx>
					<posy>5</posy>
					<texture>folder-focus.png</texture>
					<animation effect="zoom" end="0,0,110,110" time="100">focus</animation>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>10</posy>
					<width>90</width>
					<height>90</height>
					<info>ListItem.Icon</info>
					<animation effect="zoom" end="5,5,100,100" time="100">focus</animation>
				</control>
				<control type="image">
					<posx>80</posx>
					<posy>80</posy>
					<width>28</width>
					<height>28</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="image">
					<width>100</width>
					<height>100</height>
					<posx>5</posx>
					<posy>5</posy>
					<texture>folder-focus2.png</texture>
					<animation effect="zoom" end="0,0,110,110" time="100">focus</animation>
				</control>
				<control type="label">
					<posx>55</posx>
					<posy>108</posy>
					<width>100</width>
					<height>22</height>
					<font>font13</font>
					<textcolor>button-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
	</include>
