<includes>
	<include name="WrapView">
		<control type="wraplist" id="54">
			<viewtype label="31943">Wraplist</viewtype>
			<posx>-80</posx>
			<posy>150</posy>
			<width>1000</width>
			<height>420</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>1354</ondown>
			<include>list-fade</include>
			<focusposition>2</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>6</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>1354</pagecontrol>
			<itemlayout height="390" width="140">
				<control type="image">
					<posx>-15</posx>
					<posy>155</posy>
					<width>140</width>
					<height>140</height>
					<info>ListItem.Icon</info>
					<texture background="true" diffuse="mask-rect.png">-</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>keep</aspectratio>
				</control>
				<control type="image">
					<posx>-15</posx>
					<posy>295</posy>
					<width>140</width>
					<height>140</height>
					<texture background="true" flipy="true" diffuse="mask-fade.png">-</texture>
					<info>ListItem.Icon</info>
					<aspectratio>keep</aspectratio>
				</control>
				<control type="image">
					<posx>121</posx>
					<posy>271</posy>
					<width>25</width>
					<height>25</height>
					<info>ListItem.Overlay</info>
					<colordiffuse>UnFocusedCovers</colordiffuse>
				</control>
				<control type="image">
					<posx>121</posx>
					<posy>296</posy>
					<width>25</width>
					<height>25</height>
					<texture flipy="true" diffuse="mask-fade.png">-</texture>
					<info>ListItem.Overlay</info>
				</control>
			</itemlayout>
			<focusedlayout height="410" width="320">
				<control type="image">
					<posx>15</posx>
					<posy>55</posy>
					<width>240</width>
					<height>240</height>
					<aspectratio>keep</aspectratio>
					<info>ListItem.Icon</info>
					<texture background="true" diffuse="mask-rect.png">-</texture>
				</control>
				<control type="image">
					<posx>15</posx>
					<posy>295</posy>
					<width>240</width>
					<height>240</height>
					<texture background="true" flipy="true" diffuse="mask-fade.png">-</texture>
					<aspectratio>keep</aspectratio>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>250</posx>
					<posy>297</posy>
					<width>60</width>
					<height>60</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="image">
					<posx>249</posx>
					<posy>353</posy>
					<width>60</width>
					<height>60</height>
					<texture flipy="true" diffuse="mask-fade.png">-</texture>
					<info>ListItem.Overlay</info>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<!-- Scroll Bar -->
			<control type="scrollbar" id="1354">
				<posx>10</posx>
				<posy>490</posy>
				<width>700</width>
				<height>22</height>
				<texturesliderbackground border="10">scrollbar_background.png</texturesliderbackground>
				<texturesliderbar border="10">scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus border="10">scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<onup>54</onup>
				<ondown>2</ondown>
				<include>list-fade</include>
				<showonepage>false</showonepage>
				<orientation>horizontal</orientation>
				<colordiffuse>ScrollBarColor</colordiffuse>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>10</posx>
				<posy>510</posy>
				<width>700</width>
				<height>30</height>
				<font>font20</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Label -->
			<control type="label">
				<posx>10</posx>
				<posy>510</posy>
				<width>700</width>
				<height>30</height>
				<font>font20</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
		</control>
	</include>
</includes>
