<includes>
	<include name="WallView">
		<control type="panel" id="58">
			<posx>136</posx>
			<posy>152</posy>
			<width>1008</width>
			<height>432</height>
			<onleft>9000</onleft>
			<onright>1358</onright>
			<onup>58</onup>
			<ondown>58</ondown>
			<viewtype label="31945">Wall</viewtype>
			<animation effect="rotatey" start="-15" end="-15" time="0" condition="Skin.HasSetting(tilted-videowall)">Conditional</animation>
			<scrolltime>200</scrolltime>
			<include>list-fade</include>
			<pagecontrol>1358</pagecontrol>
			<preloaditems>8</preloaditems>
			<itemlayout height="144" width="144">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>144</width>
					<height>144</height>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<texture background="true" diffuse="mask-squ.png">DefaultProgram.png</texture>
					<info>ListItem.Icon</info>
				</control>
			</itemlayout>
			<focusedlayout height="144" width="144">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>144</width>
					<height>144</height>
					<texture background="true" diffuse="mask-squ.png">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<bordertexture border="9">mask-hilite.png</bordertexture>
					<bordersize>9</bordersize>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="140" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="140" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<visible>SubString(ListItem.Icon,Default,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(58)</visible>
			<!-- Title -->
			<control type="label">
				<posx>200</posx>
				<posy>625</posy>
				<width>880</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>200</posx>
				<posy>625</posy>
				<width>880</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<control type="scrollbar" id="1358">
				<posx>1240</posx>
				<posy>102</posy>
				<width>24</width>
				<height>490</height>
				<texturesliderbackground border="0,8,0,8">scrollbar_background.png</texturesliderbackground>
				<texturesliderbar border="0,8,0,8">scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,8,0,8">scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<onleft>58</onleft>
				<onright>9000</onright>
				<include>list-fade</include>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<colordiffuse>ScrollBarColor</colordiffuse>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
		</control>
	</include>
</includes>
