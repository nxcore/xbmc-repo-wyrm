<includes>
	<include name="VideoWrapView">
		<control type="wraplist" id="54">
			<viewtype label="31942">Wraplist</viewtype>
			<posx>0</posx>
			<posy>187</posy>
			<width>1258</width>
			<height>525</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>9000</onup>
			<ondown>24</ondown>
			<include>list-fade</include>
			<focusposition>0</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>13</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>24</pagecontrol>
			<itemlayout height="292" width="90">
				<control type="image">
					<posx>-5</posx>
					<posy>336</posy>
					<height>128</height>
					<width>79</width>
					<texture>case-dvd.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-5</posx>
					<posy>340</posy>
					<height>120</height>
					<width>77</width>
					<texture diffuse="mask-rect.png">DefaultVideo.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-5</posx>
					<posy>340</posy>
					<height>120</height>
					<width>77</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$VAR[IconPoster]</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-4</posx>
					<posy>320</posy>
					<height>18</height>
					<width>26</width>
					<texture>$VAR[Overlay]</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="405" width="276">
				<control type="image">
					<posx>25</posx>
					<posy>150</posy>
					<height>321</height>
					<width>207</width>
					<texture>case-dvd.png</texture>
					<bordertexture border="16">mask-hilite.png</bordertexture>
					<bordersize>10</bordersize>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>26</posx>
					<posy>148</posy>
					<height>312</height>
					<width>203</width>
					<aspectratio align="center" aligny="center">stretch</aspectratio>
					<texture background="true" diffuse="mask-rect.png">DefaultVideo.png</texture>
					<info>ListItem.Icon</info>
					<visible>SubString(ListItem.Icon,Default,Left) + ![SubString(ListItem.Icon,DefaultFolder.png,Left)]</visible>
				</control>
				<control type="image">
					<posx>26</posx>
					<posy>148</posy>
					<height>312</height>
					<width>203</width>
					<texture diffuse="mask-rect.png">DefaultVideo.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>35</posx>
					<posy>164</posy>
					<height>292</height>
					<width>183</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$VAR[IconPoster]</texture>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>28</posx>
					<posy>144</posy>
					<height>328</height>
					<width>213</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$VAR[IconPoster]</texture>
					<bordertexture border="16">mask-hilite.png</bordertexture>
					<bordersize>10</bordersize>
					<aspectratio>stretch</aspectratio>
					<visible>!Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>430</posx>
					<posy>140</posy>
					<height>48</height>
					<width>48</width>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<!-- Scroll Bar -->
			<control type="scrollbar" id="24">
				<posx>18</posx>
				<posy>658</posy>
				<width>1240</width>
				<height>22</height>
				<onup>54</onup>
				<ondown>9000</ondown>
				<include>list-fade</include>
				<orientation>horizontal</orientation>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<!-- Star Rating -->
			<control type="image">
				<posx>234</posx>
				<posy>335</posy>
				<width>200</width>
				<height>25</height>
				<info>ListItem.StarRating</info>
				<colordiffuse>StarDiffuse</colordiffuse>
				<visible>!IsEmpty(ListItem.Thumb)</visible>
				<visible>!IsEmpty(ListItem.StarRating)</visible>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- MPAA Rating -->
			<control type="image">
				<posx>272</posx>
				<posy>367</posy>
				<width>212</width>
				<height>62</height>
				<aligny>bottom</aligny>
				<texture>$VAR[VideoRating]</texture>
				<aspectratio>keep</aspectratio>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>272</posx>
				<posy>429</posy>
				<width>929</width>
				<height>37</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>stringcompare(ListItem.Icon,DefaultFolderBack.png)</visible>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Title -->
			<control type="label">
				<posx>272</posx>
				<posy>429</posy>
				<width>929</width>
				<height>37</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!stringcompare(ListItem.Icon,DefaultFolderBack.png)</visible>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Genre -->
			<control type="label">
				<posx>272</posx>
				<posy>474</posy>
				<width>929</width>
				<height>37</height>
				<font>font13</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<selectedcolor>listselectedtext</selectedcolor>
				<scroll>true</scroll>
				<info>ListItem.Genre</info>
				<textoffsetx>0</textoffsetx>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
		</control>
	</include>
</includes>
