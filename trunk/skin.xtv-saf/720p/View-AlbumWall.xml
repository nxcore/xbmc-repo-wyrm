<includes>
	<include name="AudioWallView">
		<control type="panel" id="58">
			<posx>136</posx>
			<posy>137</posy>
			<width>1008</width>
			<height>432</height>
			<onleft>9000</onleft>
			<onright>1258</onright>
			<onup>58</onup>
			<ondown>58</ondown>
			<viewtype label="31944">Wall</viewtype>
			<animation effect="rotatey" start="-15" end="-15" time="0" condition="Skin.HasSetting(tilted-videowall)">Conditional</animation>
			<scrolltime>200</scrolltime>
			<include>list-fade</include>
			<pagecontrol>1258</pagecontrol>
			<itemlayout height="144" width="144">
				<control type="image">
					<width>144</width>
					<height>144</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<aspectratio align="center" aligny="center">stretch</aspectratio>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<texture diffuse="mask-squ.png" background="true" fallback="DefaultAudio.png">$INFO[ListItem.Icon]</texture>
					<info>ListItem.Icon</info>
				</control>
			</itemlayout>
			<focusedlayout height="144" width="144">
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<texture background="true" diffuse="mask-squ.png" fallback="DefaultAudio.png">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="center">stretch</aspectratio>
					<bordertexture border="9">mask-hilite.png</bordertexture>
					<bordersize>9</bordersize>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="150" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>8</posx>
					<posy>8</posy>
					<width>128</width>
					<height>128</height>
					<aspectratio align="center" aligny="center">stretch</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="90" end="150" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<texture background="true" fallback="DefaultAudio.png">$INFO[ListItem.Icon]</texture>
					<visible>SubString(ListItem.Icon,Default,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(58)</visible>
			<control type="scrollbar" id="1258">
				<posx>1240</posx>
				<posy>112</posy>
				<width>22</width>
				<height>490</height>
				<onleft>58</onleft>
				<onright>9000</onright>
				<include>list-fade</include>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>26</posx>
				<posy>645</posy>
				<width>1221</width>
				<height>37</height>
				<font>font20</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<control type="group">
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
				<!-- Artist & Album name-->
				<control type="label">
					<posx>26</posx>
					<posy>640</posy>
					<width>1228</width>
					<height>36</height>
					<font>font20</font>
					<align>center</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<scroll>true</scroll>
					<label>$INFO[ListItem.Artist,, - ]$INFO[ListItem.Album]</label>
					<textoffsetx>0</textoffsetx>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<!-- Icon label for Default icons -->
				<control type="label">
					<posx>26</posx>
					<posy>645</posy>
					<width>1221</width>
					<height>37</height>
					<font>font20</font>
					<align>center</align>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<scroll>true</scroll>
					<info>ListItem.Label</info>
					<textoffsetx>0</textoffsetx>
					<visible>SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<!-- Star Rating -->
				<control type="image">
					<posx>26</posx>
					<posy>685</posy>
					<width>177</width>
					<height>25</height>
					<info>ListItem.StarRating</info>
					<colordiffuse>StarDiffuse</colordiffuse>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
					<visible>!IsEmpty(ListItem.StarRating)</visible>
				</control>
				<!-- Genre -->
				<control type="label">
					<posx>240</posx>
					<posy>685</posy>
					<width>800</width>
					<height>37</height>
					<font>font13</font>
					<align>center</align>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<scroll>true</scroll>
					<label>$INFO[ListItem.Genre]</label>
					<textoffsetx>0</textoffsetx>
					<visible>!Container.Content(genres)</visible>
				</control>
			</control>	
		</control>
	</include>
</includes>
