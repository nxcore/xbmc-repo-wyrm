<window type="dialog" id="114">
	<defaultcontrol always="false">214</defaultcontrol>
	<animation effect="fade" start="0" time="100">WindowOpen</animation>
	<animation effect="fade" end="0" time="200">WindowClose</animation>
	<visible>Player.HasMedia + Window.IsActive(PlayerControls) + !Window.IsActive(FullscreenVideo) + !Window.IsActive(Visualisation) + !Window.IsActive(karaoke) + !Window.IsActive(visualisationpresetlist)</visible>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
		<control type="image">
			<description>Dim current screen</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>background.png</texture>
			<colordiffuse>osd-background</colordiffuse>
		</control>
		<control type="image">
			<description>Player Controls background image</description>
			<posx>123</posx>
			<posy>125</posy>
			<height>175</height>
			<width>1026</width>
			<texture border="8" diffuse="mask-squ.png">button-menu-background.png</texture>
			<colordiffuse>solid-background</colordiffuse>
		</control>
		<!-- Media Icon -->
		<control type="image">
			<description>Video cover image</description>
			<posx>132</posx>
			<posy>137</posy>
			<width>212</width>
			<height>150</height>
			<align>left</align>
			<aligny>bottom</aligny>
			<fadetime>300</fadetime>
			<info>VideoPlayer.Cover</info>
			<aspectratio>keep</aspectratio>
		</control>
		<control type="image">
			<description>Music cover image</description>
			<posx>132</posx>
			<posy>137</posy>
			<width>212</width>
			<height>150</height>
			<align>left</align>
			<aligny>bottom</aligny>
			<fadetime>300</fadetime>
			<info>MusicPlayer.Cover</info>
			<aspectratio>keep</aspectratio>
			<visible>!Window.IsActive(visualisation)</visible>
		</control>
		<!-- End of Media Icon -->
		<!-- Media Info -->
		<!-- Video -->
		<control type="label">
			<description>Now playing</description>
			<posx>354</posx>
			<posy>137</posy>
			<width>531</width>
			<height>31</height>
			<label>13350</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>listselectedtext</textcolor>
			<visible>Player.HasVideo</visible>
		</control>
		<control type="label">
			<description>Video Name</description>
			<posx>354</posx>
			<posy>168</posy>
			<width>531</width>
			<height>31</height>
			<label>$INFO[VideoPlayer.Title]</label>
			<font>font12</font>
			<align>left</align>
		</control>
		<control type="label">
			<description>Time label</description>
			<posx>354</posx>
			<posy>200</posy>
			<width>250</width>
			<height>31</height>
			<label>$INFO[VideoPlayer.Time]$INFO[VideoPlayer.Duration, / ]</label>
			<align>left</align>
			<font>font12</font>
			<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
			<visible>Player.HasVideo</visible>
		</control>
		<control type="progress">
			<description>Progressbar</description>
			<posx>604</posx>
			<posy>208</posy>
			<width>500</width>
			<height>16</height>
			<info>Player.Progress</info>
			<texturebg border="14,0,14,0">progress-background.png</texturebg>
			<midtexture border="14,0,14,0">progress-middle.png</midtexture>
			<colordiffuse>ProgressBarColor</colordiffuse>
			<visible>Player.HasVideo</visible>
		</control>
		<control type="slider">
			<description>Seek Slider</description>
			<posx>604</posx>
			<posy>206</posy>
			<width>500</width>
			<height>20</height>
			<texturesliderbar border="8,0,8,0">seekslider.png</texturesliderbar>
			<textureslidernib>progress-marker.png</textureslidernib>
			<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
			<action>seek</action>
			<visible>Player.HasVideo</visible>
		</control>
		<!-- Music -->
		<control type="label">
			<description>Album Artist</description>
			<posx>354</posx>
			<posy>132</posy>
			<width>778</width>
			<height>20</height>
			<label>$INFO[MusicPlayer.Artist]</label>
			<font>font12</font>
			<align>left</align>
		</control>
		<control type="label">
			<description>Album Name</description>
			<posx>354</posx>
			<posy>160</posy>
			<width>778</width>
			<height>20</height>
			<label>$INFO[MusicPlayer.Album]</label>
			<font>font12</font>
			<align>left</align>
		</control>
		<control type="label">
			<description>Track Name</description>
			<posx>354</posx>
			<posy>188</posy>
			<width>778</width>
			<height>20</height>
			<label>$INFO[MusicPlayer.Title]</label>
			<font>font12</font>
			<align>left</align>
		</control>
		<control type="label">
			<description>Duration Label</description>
			<posx>354</posx>
			<posy>216</posy>
			<label>$INFO[MusicPlayer.Time]$INFO[MusicPlayer.Duration, / ]</label>
			<font>font12</font>
			<align>left</align>
			<width>354</width>
			<height>20</height>
			<visible>Player.HasAudio</visible>
		</control>
		<control type="progress">
			<description>Progressbar</description>
			<posx>520</posx>
			<posy>223</posy>
			<width>440</width>
			<height>16</height>
			<info>Player.Progress</info>
			<texturebg border="14,0,14,0">progress-background.png</texturebg>
			<midtexture border="14,0,14,0">progress-middle.png</midtexture>
			<colordiffuse>ProgressBarColor</colordiffuse>
			<visible>Player.HasAudio</visible>
		</control>
		<control type="slider">
			<description>Seek Slider</description>
			<posx>520</posx>
			<posy>221</posy>
			<width>440</width>
			<height>20</height>
			<texturesliderbar border="8,0,8,0">seekslider.png</texturesliderbar>
			<textureslidernib>progress-marker.png</textureslidernib>
			<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
			<action>seek</action>
			<visible>Player.HasAudio</visible>
		</control>
		<!-- End of Media Info-->
		<!-- Player buttons -->
		<control type="grouplist" id="999">
			<description>Group OSD Button</description>
			<posx>354</posx>
			<posy>245</posy>
			<width>778</width>
			<height>40</height>
			<itemgap>3</itemgap>
			<scrolltime>200</scrolltime>
			<orientation>horizontal</orientation>
			<align>center</align>
			<usecontrolcoords>false</usecontrolcoords>
			<onleft>999</onleft>
			<onright>999</onright>
			<onup>999</onup>
			<ondown>999</ondown>
			<visible>true</visible>
			<control type="togglebutton" id="908">
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-playthru-focus.png</texturefocus>
				<texturenofocus>osd-btn-playthru.png</texturenofocus>
				<usealttexture>Playlist.IsRandom</usealttexture>
				<alttexturefocus>osd-btn-random-focus.png</alttexturefocus>
				<alttexturenofocus>osd-btn-random.png</alttexturenofocus>
				<onclick>XBMC.PlayerControl(Random)</onclick>
				<visible>Player.HasAudio</visible>
			</control>
			<control type="group" id="925">
				<width>40</width>
				<height>40</height>
				<visible>Player.HasAudio</visible>
				<control type="button" id="912">
					<width>40</width>
					<height>40</height>
					<onleft>908</onleft>
					<onright>217</onright>
					<texturefocus>-</texturefocus>
					<texturenofocus>-</texturenofocus>
					<onclick>PlayerControl(Repeat)</onclick>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeat.png</texture>
					<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
					<visible>!Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeat-focus.png</texture>
					<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
					<visible>Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatone.png</texture>
					<visible>Playlist.IsRepeatOne</visible>
					<visible>!Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatone-focus.png</texture>
					<visible>Playlist.IsRepeatOne</visible>
					<visible>Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatall.png</texture>
					<visible>Playlist.IsRepeat</visible>
					<visible>!Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatall-focus.png</texture>
					<visible>Playlist.IsRepeat</visible>
					<visible>Control.HasFocus(912)</visible>
				</control>
			</control>
			<control type="image" id="930">
				<description>Dummy</description>
				<width>10</width>
				<height>40</height>
				<texture>-</texture>
			</control>	
			<control type="button" id="217">
				<description>Eject Disk</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-eject-focus.png</texturefocus>
				<texturenofocus>osd-btn-eject.png</texturenofocus>
				<onclick>XBMC.EjectTray()</onclick>
				<onclick>SetFocus(214)</onclick>
				<visible>System.HasMediadvd</visible>
			</control>
			<control type="button" id="210">
				<description>Skip Bwd</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-skipback-focus.png</texturefocus>
				<texturenofocus>osd-btn-skipback.png</texturenofocus>
				<onclick>XBMC.PlayerControl(Previous)</onclick>
			</control>
			<control type="button" id="211">
				<description>Rewind</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-rewind-focus.png</texturefocus>
				<texturenofocus>osd-btn-rewind.png</texturenofocus>
				<onclick>XBMC.PlayerControl(Rewind)</onclick>
			</control>
			<control type="togglebutton" id="213">
				<description>Play</description>
				<width>40</width>
				<height>40</height>	
				<texturefocus>osd-btn-pause-focus.png</texturefocus>
				<texturenofocus>osd-btn-pause.png</texturenofocus>
				<usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
				<alttexturefocus>osd-btn-play-focus.png</alttexturefocus>
				<alttexturenofocus>osd-btn-play.png</alttexturenofocus>
				<onclick>XBMC.PlayerControl(Play)</onclick>
			</control>
			<control type="button" id="214">
				<description>Stop</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-stop-focus.png</texturefocus>
				<texturenofocus>osd-btn-stop.png</texturenofocus>
				<onclick>XBMC.PlayerControl(Stop)</onclick>
			</control>
			<control type="button" id="215">
				<description>Fast Forward</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-fastfwd-focus.png</texturefocus>
				<texturenofocus>osd-btn-fastfwd.png</texturenofocus>
				<onclick>XBMC.PlayerControl(Forward)</onclick>
			</control>
			<control type="button" id="216">
				<description>Skip Fwd</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-skipfwd-focus.png</texturefocus>
				<texturenofocus>osd-btn-skipfwd.png</texturenofocus>
				<onclick>XBMC.PlayerControl(Next)</onclick>
			</control>
			<control type="button" id="3">
				<description>Record Button</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-rec-focus.png</texturefocus>
				<texturenofocus>osd-btn-rec.png</texturenofocus>
				<onclick>XBMC.PlayerControl(Record)</onclick>
				<visible>Player.CanRecord</visible>
			</control>
			<control type="image" id="940">
				<description>Dummy</description>
				<width>10</width>
				<height>40</height>
				<texture>-</texture>
			</control>
			<control type="button" id="220">
				<description>Popup Vis Preset List</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-vis-focus.png</texturefocus>
				<texturenofocus>osd-btn-vis.png</texturenofocus>
				<onclick>XBMC.ActivateWindow(visualisationpresetlist)</onclick>
				<visible>Player.HasAudio</visible>
			</control>
			<control type="button" id="219">
				<description>Popup Vis Settings</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-setting-focus.png</texturefocus>
				<texturenofocus>osd-btn-setting.png</texturenofocus>
				<onclick>Addon.Default.OpenSettings(xbmc.player.musicviz)</onclick>
				<visible>Player.HasAudio</visible>
			</control>
		</control>
		<!-- End Player buttons -->
	</controls>
</window>