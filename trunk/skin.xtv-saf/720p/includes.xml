								<posy>-180</posy>
								<width>180</width>
								<height>180</height>
								<aligny>bottom</aligny>
								<texture diffuse="mask-rect.png">$VAR[PlayerVideoCover]</texture>
								<aspectratio scalediffuse="false">keep</aspectratio>
								<visible>StringCompare(Control.GetLabel(891),blank.png)</visible>
							</control>
						</control>
					</control>
					<control type="group">
						<visible>VideoPlayer.Content(episodes)</visible><!-- tvshows -->
						<animation effect="fade" time="200">VisibleChange</animation>
						<control type="image" id="990">
							<description>Use TV Clearart if available</description>
							<posx>8</posx>
							<posy>-138</posy>
							<width>275</width>
							<height>145</height>
							<texture>$VAR[PlayerVideoClearart]</texture>
							<aspectratio>keep</aspectratio>
						</control>
						<control type="group">
							<visible>StringCompare(Control.GetLabel(990),blank.png)</visible>
							<control type="image" id="991">
								<description>Use TV Clearlogo if available</description>
								<posx>8</posx>
								<posy>-75</posy>
								<width>276</width>
								<height>75</height>
								<texture>$VAR[PlayerVideoLogo]</texture>
								<aspectratio align="left" aligny="bottom">keep</aspectratio>
							</control>
							<control type="image">
								<description>Fallback to video cover image if no clearart or logo</description>
								<posx>56</posx>
								<posy>-180</posy>
								<width>180</width>
								<height>180</height>
								<aligny>bottom</aligny>
								<texture diffuse="mask-rect.png">$VAR[PlayerVideoCover]</texture>
								<aspectratio scalediffuse="false">keep</aspectratio>
								<visible>StringCompare(Control.GetLabel(991),blank.png)</visible>
							</control>
						</control>
					</control>
				</control>
				<control type="group"><!-- Any video other than Movies and TV Episodes -->
					<visible>![VideoPlayer.Content(Movies) | VideoPlayer.Content(episodes)]</visible>
					<animation effect="fade" time="200">VisibleChange</animation>
					<control type="image">
						<description>Fallback to Thumb image if not TV Episode or Movie playing</description>
						<posx>56</posx>
						<posy>-180</posy>
						<width>180</width>
						<height>180</height>
						<aligny>bottom</aligny>
						<texture diffuse="mask-rect.png">$INFO[Player.Art(thumb)]</texture>
						<aspectratio scalediffuse="false">keep</aspectratio>
					</control>
				</control>
			</control>
			<control type="image"><!-- Music cover image-->
				<posx>8</posx>
				<posy>-256</posy>
				<width>250</width>
				<height>250</height>
				<texture diffuse="mask-squ.png">$INFO[MusicPlayer.Cover]</texture>
