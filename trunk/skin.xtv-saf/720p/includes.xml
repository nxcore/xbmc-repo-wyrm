	<include name="fadebackground">
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>colours/black.png</texture>
			<aspectratio>scale</aspectratio>
			<colordiffuse>mask-background</colordiffuse>
		</control>
	</include>

	<include name="background">
		<control type="group">
			<animation effect="fade" start="0" end="100" time="500" delay="2500">Visible</animation>
			<animation effect="fade" start="100" end="0" time="0">Hidden</animation>
			<control type="image"><!-- Use a black background until background image has had a chance to load -->
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<texture>colours/black.png</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<texture background="true">$VAR[BackgroundImage]</texture>
				<fadetime>200</fadetime>
				<visible>![Skin.HasSetting(HomeViewToggle) + Player.HasVideo]</visible>
			</control>
			<control type="group">
				<visible>Skin.HasSetting(HomeViewToggle)</visible>
				<control type="videowindow">
					<posx>0</posx>
					<posy>0</posy>
					<width>1280</width>
					<height>720</height>
					<aspectratio>stretch</aspectratio>
					<visible>Player.HasVideo</visible>
				</control>
				<control type="visualisation">
					<posx>0</posx>
					<posy>0</posy>
					<width>1280</width>
					<height>720</height>
					<aspectratio>stretch</aspectratio>
					<visible>Player.HasAudio</visible>
				</control>
			</control>
			<control type="group">
				<visible>Skin.HasSetting(background-slideshow) + Player.HasAudio + !SubString(Window(VideoLibrary).Property(TvTunesIsAlive),True)</visible>
				<control type="image">
					<description>Mask out any backgrounds showing around ArtistSlideshow Fanart</description>
					<posx>0</posx>
					<posy>0</posy>
					<width>1280</width>
					<height>720</height>
					<texture>colours/black.png</texture>
					<aspectratio>stretch</aspectratio>
				</control>
				<control type="multiimage">
					<description>Backgroung ArtistSlideshow fanart image</description>
					<posx>0</posx>
					<posy>0</posy>
					<width>1280</width>
					<height>720</height>
					<imagepath fallback="blank.png" background="true">$INFO[Window(Visualisation).Property(ArtistSlideshow)]</imagepath>
					<aspectratio>keep</aspectratio>
					<timeperimage>5000</timeperimage>
					<fadetime>400</fadetime>
					<randomize>true</randomize>
				</control>
			</control>
		</control>
		<control type="image">
			<description>mask to fade backgrounds</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<colordiffuse>mask-background</colordiffuse>
			<texture>colours/black.png</texture>
			<animation effect="fade" start="100" end="80" condition="Skin.HasSetting(background-visibiltyhigh)">Conditional</animation>
			<animation effect="fade" start="100" end="90" condition="Skin.HasSetting(background-visibiltymed)">Conditional</animation>
			<aspectratio>stretch</aspectratio>
			<visible>!IsEmpty(Skin.String(custom-weatherbackgrounds)) | Skin.HasSetting(background-fanart) | Skin.HasSetting(HomeViewToggle) | Skin.HasSetting(background-slideshow)</visible>
		</control>
	</include>

	<include name="backgroundvideo">
		<control type="group">
			<visible>!Container().Scrolling</visible>
			<animation effect="fade" start="0" end="100" time="2500" delay="500" tween="quadratic" easing="out">Visible</animation>
			<animation effect="fade" start="100" end="0" time="0">Hidden</animation>
			<control type="image"><!-- Use a black background until background image has had a chance to load -->
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<texture>colours/black.png</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			<control type="group">
				<visible>![Skin.HasSetting(HomeViewToggle) + Player.HasVideo] + !IsEmpty(ListItem.Title)</visible>
				<control type="group">
					<visible>![Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(movies) | Container.Content(musicvideos)]</visible>
					<control type="image">
						<description>Fallback background image</description>
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<aspectratio>stretch</aspectratio>
						<fadetime>300</fadetime>
						<texture background="true">$VAR[VideoBackgndImage]</texture>
					</control>
				</control>
				<control type="group">
					<visible>[Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(musicvideos)]</visible>
					<control type="multiimage" id="6300">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<randomize>true</randomize>
						<timeperimage>5000</timeperimage>
						<fadetime>300</fadetime>
						<imagepath background="true">$INFO[ListItem.Path,,extrafanart/]</imagepath>
						<aspectratio>stretch</aspectratio>
						<visible>Skin.HasSetting(background-extrafanart)</visible>
					</control>
					<control type="image">
						<description>Fallback background image</description>
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<aspectratio>stretch</aspectratio>
						<fadetime>300</fadetime>
						<texture background="true">$VAR[VideoBackgndImage]</texture>
						<visible>!Skin.HasSetting(background-extrafanart) | IsEmpty(Control.GetLabel(6300))</visible>
					</control>
				</control>
				<control type="group">
					<visible>Container.Content(episodes)</visible>
					<control type="multiimage" id="6340">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<randomize>true</randomize>
						<timeperimage>5000</timeperimage>
						<fadetime>300</fadetime>
						<imagepath background="true">$INFO[ListItem.Path,,../extrafanart/]</imagepath>
						<aspectratio>stretch</aspectratio>
						<visible>Skin.HasSetting(background-extrafanart) + !Window.IsVisible(InfoDialog) + !IsEmpty(ListItem.Path)</visible>
					</control>
					<control type="image" id="6342">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<texture background="true">$VAR[VideoBackgndImage]</texture>
						<aspectratio>stretch</aspectratio>
						<fadetime>300</fadetime>
						<visible>IsEmpty(Control.GetLabel(6340)) | !Skin.HasSetting(background-extrafanart)</visible>
					</control>
				</control>
			</control>
		</control>
		<control type="group">
			<visible>Skin.HasSetting(HomeViewToggle)</visible>
			<control type="videowindow">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<aspectratio>stretch</aspectratio>
				<visible>Player.HasVideo</visible>
			</control>
			<control type="visualisation">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<aspectratio>stretch</aspectratio>
