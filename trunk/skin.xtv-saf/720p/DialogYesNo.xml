			<onright>10</onright>
			<onup>noop</onup>
			<ondown>noop</ondown>
		</control>
		<control type="button" id="10"><!-- NO button -->
			<posx>657</posx>
			<posy>416</posy>
			<width>265</width>
			<height>50</height>
			<label>$LOCALIZE[106]</label>
			<font>font12</font>
			<align>center</align>
			<onleft>11</onleft>
			<onright>noop</onright>
			<onup>noop</onup>
			<ondown>noop</ondown>
		</control>
