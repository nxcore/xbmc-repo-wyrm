<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">10</defaultcontrol>
	<coordinates>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<include>screen-fade</include>
	<controls>
