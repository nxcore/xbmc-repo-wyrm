<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="142">
	<coordinates>
		<posx>0</posx>
		<posy>640</posy>
	</coordinates>
	<controls>
		<control type="group">
			<visible>![Window.IsVisible(SliderDialog) | Window.IsVisible(OSDVideoSettings) | Window.IsVisible(OSDAudioSettings) | Window.IsVisible(VideoBookmarks) | Window.IsVisible(script.pseudotv.TVOverlay.xml)]</visible>
			<control type="image"><!-- Dialog background -->
				<posx>-42</posx>
				<posy>-178</posy>
				<height>264</height>
				<width>1359</width>
				<texture>background.png</texture>
				<colordiffuse>osd-background</colordiffuse>
			</control>
			<control type="group"><!-- Media Icon -->
				<visible>Window.IsVisible(FullscreenVideo)</visible>
				<control type="group"><!-- tvshows -->
					<visible>VideoPlayer.Content(episodes)</visible>
					<animation effect="fade" time="200">VisibleChange</animation>
					<control type="image" id="990"><!-- Use TV Clearart if available(episodes in season folders) -->
						<posx>8</posx>
						<posy>-154</posy>
						<width>275</width>
						<height>145</height>
						<texture fallback="blank.png">$INFO[Player.FolderPath,,../clearart.png]</texture>
						<aspectratio>keep</aspectratio>
					</control>
					<control type="image" id="991"><!-- Use TV Clearart if available(episodes in tv show folder) -->
						<posx>8</posx>
						<posy>-154</posy>
						<width>275</width>
						<height>145</height>
						<texture fallback="blank.png">$INFO[Player.FolderPath,,clearart.png]</texture>
						<aspectratio>keep</aspectratio>
						<visible>StringCompare(Control.GetLabel(990),blank.png)</visible>
					</control>
					<control type="group">
						<visible>StringCompare(Control.GetLabel(990),blank.png) + StringCompare(Control.GetLabel(991),blank.png)</visible>
						<control type="image" id="992"><!-- Otherwise try TV logo if available(episodes in season folders) -->
							<posx>8</posx>
							<posy>-75</posy>
							<width>276</width>
							<height>75</height>
							<texture fallback="blank.png">$INFO[Player.FolderPath,,logo.png]</texture>
							<aligny>bottom</aligny>
							<aspectratio align="left">keep</aspectratio>
						</control>
						<control type="image" id="993"><!-- Otherwise try TV logo if available(episodes in tv show folder) -->
							<posx>8</posx>
							<posy>-75</posy>
							<width>276</width>
							<height>75</height>
							<texture fallback="blank.png">$INFO[Player.FolderPath,,../logo.png]</texture>
							<aligny>bottom</aligny>
							<aspectratio align="left">keep</aspectratio>
							<visible>StringCompare(Control.GetLabel(992),blank.png)</visible>
						</control>
						<control type="group">
							<visible>StringCompare(Control.GetLabel(992),blank.png) + StringCompare(Control.GetLabel(993),blank.png)</visible>
							<control type="image" id="994"><!-- Otherwise try TV folder.jpg if available(episodes in season folders) -->
								<posx>56</posx>
								<posy>-180</posy>
								<width>180</width>
								<height>180</height>
								<texture fallback="blank.png" diffuse="mask-rect.png">$INFO[Player.FolderPath,,folder.jpg]</texture>
								<aligny>bottom</aligny>
								<aspectratio scalediffuse="false">keep</aspectratio>
							</control>
							<control type="image" id="995"><!-- Otherwise try TV folder.jpg if available(episodes in tv show folder) -->
								<posx>56</posx>
								<posy>-180</posy>
								<width>180</width>
								<height>180</height>
								<texture fallback="blank.png" diffuse="mask-rect.png">$INFO[Player.FolderPath,,../folder.jpg]</texture>
								<aligny>bottom</aligny>
								<aspectratio scalediffuse="false">keep</aspectratio>
								<visible>StringCompare(Control.GetLabel(994),blank.png)</visible>
							</control>
						</control>
					</control>
				</control>
				<control type="group"><!-- Movies -->
					<visible>VideoPlayer.Content(Movies)</visible>
					<animation effect="fade" time="200">VisibleChange</animation>
					<control type="image" id="890"><!-- Use Movie Clearart if available -->
						<posx>8</posx>
						<posy>-144</posy>
						<width>275</width>
						<height>145</height>
						<texture fallback="blank.png">$INFO[Player.FolderPath,,clearart.png]</texture>
						<aligny>bottom</aligny>
						<aspectratio>keep</aspectratio>
					</control>
					<control type="group">
						<visible>StringCompare(Control.GetLabel(890),blank.png)</visible>
						<control type="image" id="891"><!-- Otherwise try Movie logo if available -->
							<posx>8</posx>
							<posy>-75</posy>
							<width>276</width>
							<height>75</height>
							<texture fallback="blank.png">$INFO[Player.FolderPath,,logo.png]</texture>
							<aligny>bottom</aligny>
							<aspectratio align="left">keep</aspectratio>
							<visible>StringCompare(Control.GetLabel(890),blank.png)</visible>
						</control>
						<control type="group">
							<visible>StringCompare(Control.GetLabel(891),blank.png)</visible>
							<control type="image" id="892"><!-- Otherwise try Movie folder.jpg if available -->
								<posx>56</posx>
								<posy>-180</posy>
								<width>180</width>
								<height>180</height>
								<texture fallback="blank.png" diffuse="mask-rect.png">$INFO[Player.FolderPath,,folder.jpg]</texture>
								<aligny>bottom</aligny>
								<aspectratio scalediffuse="false">keep</aspectratio>
							</control>
							<control type="image" id="893"><!-- Fallback to cover image -->
								<posx>56</posx>
								<posy>-180</posy>
								<width>180</width>
								<height>180</height>
								<aligny>bottom</aligny>
								<texture diffuse="mask-rect.png">$INFO[VideoPlayer.cover]</texture>
								<aspectratio scalediffuse="false">keep</aspectratio>
								<visible>StringCompare(Control.GetLabel(892),blank.png)</visible>
							</control>
						</control>
					</control>
				</control>
				<control type="group"><!-- Anything other than Movies and TV Episodes -->
					<visible>![VideoPlayer.Content(Movies) | VideoPlayer.Content(episodes)]</visible>
					<animation effect="fade" time="200">VisibleChange</animation>
					<control type="image">
						<description>Fallback to Thumb image if not TV Episode or Movie playing</description>
						<posx>56</posx>
						<posy>-180</posy>
						<width>180</width>
						<height>180</height>
						<aligny>bottom</aligny>
						<texture diffuse="mask-rect.png">$INFO[Player.Art(thumb)]</texture>
						<aspectratio scalediffuse="false">keep</aspectratio>
					</control>
				</control>
			</control>
		</control>

		<control type="label"><!-- Title of currently playing video -->
			<posx>290</posx>
			<posy>-180</posy>
			<width>960</width>
			<height>25</height>
			<label>$INFO[VideoPlayer.Title]</label>
			<align>center</align>
			<visible>!VideoPlayer.Content(LiveTV)</visible>
		</control>
		<control type="group"><!-- Details of Live TV Program -->
			<visible>VideoPlayer.Content(LiveTV)</visible>
			<control type="label"><!-- TV Channel Number -->
				<posx>8</posx>
				<posy>-260</posy>
				<width>150</width>
				<height>48</height>
				<textcolor>info-text</textcolor>
				<label>$INFO[VideoPlayer.ChannelNumber]</label>
				<font>font28</font>
			</control>
			<control type="label"><!-- TV Channel Name -->
				<posx>8</posx>
				<posy>-210</posy>
				<width>400</width>
				<height>25</height>
				<label>$INFO[VideoPlayer.ChannelName]</label>
			</control>
			<control type="label"><!-- Title of Live TV Program -->
				<posx>290</posx>
				<posy>-180</posy>
				<width>475</width>
				<height>25</height>
				<label>$INFO[VideoPlayer.Title,$LOCALIZE[19030]: ,]</label>
			</control>
			<control type="label"><!-- Tv Show title up next -->
				<posx>1250</posx>
				<posy>-180</posy>
				<width>475</width>
				<height>25</height>
				<label>$INFO[VideoPlayer.NextTitle,$LOCALIZE[19031]: ,]</label>
				<align>right</align>
				<textcolor>disabled-text</textcolor>
			</control>
		</control>
		<control type="image">
			<posx>290</posx>
			<posy>-152</posy>
			<width>960</width>
			<height>5</height>
			<texture>separator.png</texture>
		</control>
		<control type="textbox"><!-- Plot (Show if available)-->
			<posx>290</posx>
			<posy>-146</posy>
			<width>960</width>
			<height>104</height>
			<font>font11</font>
			<label>$INFO[VideoPlayer.Plot]</label>
			<align>justify</align>
			<autoscroll time="4000" delay="12000" repeat="8000">Skin.HasSetting(autoscroll)</autoscroll>
			<visible>!IsEmpty(VideoPlayer.Plot)</visible>
		</control>
		<control type="textbox"><!-- Plot Outline (Show if plot not found)-->
			<posx>290</posx>
			<posy>-146</posy>
			<width>960</width>
			<height>104</height>
			<font>font11</font>
			<label>$INFO[VideoPlayer.PlotOutline]</label>
			<align>justify</align>
			<autoscroll time="4000" delay="6000" repeat="8000">Skin.HasSetting(autoscroll)</autoscroll>
			<visible>!IsEmpty(VideoPlayer.PlotOutline) + IsEmpty(VideoPlayer.Plot)</visible>
		</control>

		<control type="grouplist"><!-- Media flags -->
			<description>Group Media Flags</description>
			<posx>804</posx>
			<posy>-40</posy>
			<width>453</width>
			<height>50</height>
			<align>right</align>
			<orientation>horizontal</orientation>
			<itemgap>-8</itemgap>
			<!-- Hack to check if media flag image is available -->
			<control type="image" id="161">
				<width>1</width>
				<height>50</height>
				<colordiffuse>00FFFFFF</colordiffuse>
				<texture fallback="blank.png">$INFO[VideoPlayer.AudioCodec,special://skin/extras/images/audio/,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.AudioCodec)</visible>
			</control>
			<control type="image" id="162">
				<width>1</width>
				<height>50</height>
				<colordiffuse>00FFFFFF</colordiffuse>
				<texture fallback="blank.png">$INFO[VideoPlayer.VideoCodec,special://skin/extras/images/video/,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoCodec)</visible>
			</control>
			<control type="image" id="163">
				<width>1</width>
				<height>50</height>
				<texture fallback="blank.png">$INFO[VideoPlayer.VideoAspect,special://skin/extras/images/aspectratio/,.png]</texture>
				<colordiffuse>00FFFFFF</colordiffuse>
				<visible>!IsEmpty(VideoPlayer.VideoAspect)</visible>
			</control>
			<control type="image" id="164">
				<width>1</width>
				<height>50</height>
				<colordiffuse>00FFFFFF</colordiffuse>
				<texture fallback="blank.png">$INFO[VideoPlayer.VideoResolution,special://skin/extras/images/resolution/,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoResolution)</visible>
			</control>
			<control type="image" id="171"><!-- Audio Codec -->
				<width>67</width>
				<height>50</height>
				<colordiffuse>FlagDiffuse</colordiffuse>
				<texture>$INFO[VideoPlayer.AudioCodec,special://skin/extras/images/audio/,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.AudioCodec) + !StringCompare(Control.GetLabel(161),blank.png)</visible>
			</control>
			<control type="image" id="172"><!-- Video Codec -->
				<width>67</width>
				<height>50</height>
				<colordiffuse>FlagDiffuse</colordiffuse>
				<texture fallback="blank.png">$INFO[VideoPlayer.VideoCodec,special://skin/extras/images/video/,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoCodec) + !StringCompare(Control.GetLabel(162),blank.png)</visible>
			</control>
			<control type="image" id="173"><!-- Video Aspect Ratio -->
				<width>67</width>
				<height>50</height>
				<texture>$INFO[VideoPlayer.VideoAspect,special://skin/extras/images/aspectratio/,.png]</texture>
				<colordiffuse>FlagDiffuse</colordiffuse>
				<visible>!IsEmpty(VideoPlayer.VideoAspect) + !StringCompare(Control.GetLabel(163),blank.png)</visible>
			</control>
			<control type="image" id="174"><!-- Video Resolution -->
				<width>67</width>
				<height>50</height>
				<colordiffuse>FlagDiffuse</colordiffuse>
				<texture>$INFO[VideoPlayer.VideoResolution,special://skin/extras/images/resolution/,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoResolution) + !StringCompare(Control.GetLabel(164),blank.png)</visible>
			</control>
		</control>
		<control type="group">
			<visible>[Window.IsVisible(FullscreenVideo) | Window.IsVisible(Visualisation)]</visible>
			<control type="grouplist">
				<posx>408</posx>
				<posy>17</posy>
				<width>464</width>
				<height>64</height>
				<align>center</align>
				<aligny>center</aligny>
				<orientation>horizontal</orientation>
				<control type="image">
					<width>64</width>
					<height>64</height>
					<texture>$VAR[PlayStateImage]</texture>
				</control>
				<control type="label">
					<description>Video screen headings</description>
					<width min="150" max="400">auto</width>
					<height>64</height>
					<label>$VAR[PlayStateTxt]</label>
					<font>font22</font>
				</control>
			</control>
		</control>

		<control type="group">
			<visible>[Window.IsVisible(FullscreenVideo)]</visible>
			<visible>![Window.IsVisible(SliderDialog) | Window.IsVisible(OSDVideoSettings) | Window.IsVisible(OSDAudioSettings) | Window.IsVisible(VideoBookmarks) | Window.IsVisible(script.pseudotv.TVOverlay.xml)]</visible>
			<control type="label">
				<posx>224</posx>
				<posy>5</posy>
				<width>220</width>
				<height>31</height>
				<font>font11</font>
				<label>($INFO[System.Time]) $INFO[VideoPlayer.Time]</label>
				<align>right</align>
			</control>
			<control type="label">
				<posx>1048</posx>
				<posy>5</posy>
				<width>220</width>
				<height>31</height>
				<font>font11</font>
				<label>- $INFO[VideoPlayer.TimeRemaining] ($INFO[Player.FinishTime])</label>
			</control>

			<control type="progress">
				<posx>238</posx>
				<posy>12</posy>
				<width>800</width>
				<height>16</height>
				<info>Player.ProgressCache</info>
				<colordiffuse>FF777777</colordiffuse>
			</control>
			<control type="progress">
				<posx>238</posx>
				<posy>12</posy>
				<width>800</width>
				<height>16</height>
				<info>Player.Progress</info>
				<colordiffuse>ProgressBarColor</colordiffuse>
			</control>
			<control type="slider">
				<description>Seek Slider</description>
				<posx>238</posx>
				<posy>10</posy>
				<width>800</width>
				<height>20</height>
				<info>Player.Progress</info>
			</control>
		</control>
	</controls>
</window>
