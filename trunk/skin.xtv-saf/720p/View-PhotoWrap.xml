<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="PhotoWrap">
		<control type="wraplist" id="54">
			<viewtype label="31942">Wraplist</viewtype>
			<left>10</left>
			<top>193</top>
			<width>1256</width>
			<height>500</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>9000</onup>
			<ondown>1354</ondown>
			<focusposition>2</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>2</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>1354</pagecontrol>
			<include>transition-fade</include>
			<itemlayout height="202" width="202">
				<control type="image">
					<left>16</left>
					<top>232</top>
					<width>175</width>
					<height>180</height>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<fadetime>300</fadetime>
					<aspectratio aligny="bottom">keep</aspectratio>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
				<control type="image">
					<left>16</left>
					<top>232</top>
					<width>175</width>
					<height>180</height>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio aligny="bottom">keep</aspectratio>
					<bordertexture border="6">picturepanel.png</bordertexture>
					<bordersize>6</bordersize>
					<fadetime>300</fadetime>
					<visible>!SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="420" width="428">
				<control type="image">
					<left>14</left>
					<top>0</top>
					<height>412</height>
					<width>403</width>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio aligny="bottom">keep</aspectratio>
					<bordertexture border="6">picturepanel.png</bordertexture>
					<bordersize>6</bordersize>
					<fadetime>300</fadetime>
					<visible>!SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
				<control type="image">
					<left>14</left>
					<top>0</top>
					<height>412</height>
					<width>403</width>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio aligny="bottom">keep</aspectratio>
					<fadetime>300</fadetime>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>transition-fade</include>
			<include>SortLetterNotification</include>
			<control type="scrollbar" id="1354"><!-- Scroll Bar -->
				<left>17</left>
				<top>614</top>
				<width>1239</width>
				<height>18</height>
				<onup>54</onup>
				<ondown>9000</ondown>
				<orientation>horizontal</orientation>
				<animation condition="!Control.HasFocus(1354) + !Container.Scrolling" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
				<visible>Skin.HasSetting(sbar) + SubString(Container.Viewmode,Wrap,Left)</visible>
			</control>
			<control type="label"><!-- Go Back -->
				<left>17</left>
				<top>633</top>
				<width>1239</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>$LOCALIZE[31688]</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<control type="label"><!-- Label -->
				<left>17</left>
				<top>633</top>
				<width>1239</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>$INFO[ListItem.Label]</label>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<control type="label"><!-- Resolution -->
				<left>283</left>
				<top>683</top>
				<width>708</width>
				<height>37</height>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>$INFO[ListItem.PictureResolution]</label>
				<textoffsetx>0</textoffsetx>
			</control>
		</control>
	</include>
</includes>
