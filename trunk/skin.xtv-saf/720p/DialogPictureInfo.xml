<window id="139">
	<defaultcontrol>5</defaultcontrol>
	<backgroundcolor>0</backgroundcolor>
	<include>screen-fade</include>
	<controls>
		<include>background</include>
		<control type="list" id="5">
			<posx>530</posx>
			<posy>112</posy>
			<width>708</width>
			<height>500</height>
			<onleft>23</onleft>
			<onright>23</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<scrolltime>200</scrolltime>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="46">
				<control type="label">
					<posx>16</posx>
					<posy>-4</posy>
					<width>256</width>
					<height>46</height>
					<font>font11</font>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>278</posx>
					<posy>-4</posy>
					<width>414</width>
					<height>46</height>
					<font>font11</font>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<align>left</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="46">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>708</width>
					<height>46</height>
					<texture>default-button.png</texture>
				</control>
				<control type="label">
					<posx>16</posx>
					<posy>-4</posy>
					<width>256</width>
					<height>46</height>
					<font>font11</font>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>278</posx>
					<posy>-4</posy>
					<width>414</width>
					<height>46</height>
					<font>font11</font>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<align>left</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>
        <control type="image">
			<posx>44</posx>
			<posy>206</posy>
			<width>477</width>
			<height>337</height>
			<texture background="true">$INFO[ListItem.Icon]</texture>
			<aspectratio align="middle" aligny="center">keep</aspectratio>
			<bordertexture border="6">picturepanel.png</bordertexture>
			<bordersize>6</bordersize>
			<visible>!IsEmpty(ListItem.Thumb)</visible>
        </control>
		<!-- Scrollbar -->
		<control type="scrollbar" id="23">
			<posx>1247</posx>
			<posy>112</posy>
			<width>24</width>
			<height>500</height>
			<texturesliderbackground border="0,8,0,8">scrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="0,8,0,8">scrollbar_bar.png</texturesliderbar>
			<texturesliderbarfocus border="0,8,0,8">scrollbar_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<onleft>5</onleft>
			<onright>5</onright>
			<colordiffuse>ScrollBarColor</colordiffuse>
			<include>list-fade</include>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<visible>Skin.HasSetting(sbar)</visible>
		</control>
	</controls>
</window>