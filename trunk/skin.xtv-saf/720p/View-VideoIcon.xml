<includes>
	<include name="VideoIconView">
		<control type="panel" id="52">
			<posx>20</posx>
			<posy>100</posy>
			<width>1200</width>
			<height>575</height>
			<onleft>9000</onleft>
			<onright>29</onright>
			<onup>52</onup>
			<ondown>52</ondown>
			<viewtype label="31943">Icons</viewtype>
			<scrolltime>200</scrolltime>
			<pagecontrol>29</pagecontrol>
			<itemlayout height="234" width="300">
				<control type="image">
					<posx>90</posx>
					<posy>5</posy>
					<width>120</width>
					<height>168</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>UnFocusedPosters</colordiffuse>
					<texture background="true" diffuse="mask-rect.png">DefaultVideo.png</texture>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>90</posx>
					<posy>5</posy>
					<width>120</width>
					<height>168</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<colordiffuse>UnFocusedPosters</colordiffuse>
					<texture background="true" diffuse="mask-rect.png">$VAR[IconPoster]</texture>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>224</posx>
					<posy>10</posy>
					<width>32</width>
					<height>32</height>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>150</posx>
					<posy>185</posy>
					<width>280</width>
					<height>22</height>
					<font>font11</font>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="234" width="300">
				<control type="image">
					<posx>90</posx>
					<posy>5</posy>
					<width>120</width>
					<height>168</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="124" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<texture background="true" diffuse="mask-rect.png">$VAR[IconPoster]</texture>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>90</posx>
					<posy>5</posy>
					<width>120</width>
					<height>168</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="124" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<texture background="true" diffuse="mask-rect.png">DefaultVideo.png</texture>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>90</posx>
					<posy>5</posy>
					<width>120</width>
					<height>168</height>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$VAR[IconPoster]</texture>
					<bordertexture border="10">mask-hilite.png</bordertexture>
					<bordersize>9</bordersize>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="124" center="auto" time="600" tween="back" easing="out" />
					</animation>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>224</posx>
					<posy>10</posy>
					<width>32</width>
					<height>32</height>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>150</posx>
					<posy>185</posy>
					<width>280</width>
					<height>22</height>
					<font>font11</font>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="scrollbar" id="29">
			<posx>1244</posx>
			<posy>100</posy>
			<width>22</width>
			<height>588</height>
			<onleft>52</onleft>
			<onright>9000</onright>
			<visible>Skin.HasSetting(sbar) + Control.IsVisible(52)</visible>
		</control>
	</include>
</includes>
