<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="skin.xtv-saf"
  version="1.0.0"
  name="xTV-SAF"
  provider-name="Wyrm">
  <requires>
    <import addon="xbmc.gui" version="2.11"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultresolution="PAL"
    defaultresolutionwide="PAL16x9"
    defaultthemename="Textures.xbt"
    effectslowdown="1.00"
    debugging="false"/>
  <extension point="xbmc.addon.metadata">
    <summary>xTV-SAF is a update of xTV (2269)</summary>
    <description>xTV-SAF is a fork of xTV to include the latest xbmc features in a Spouse friendly manner.</description>
    <disclaimer></disclaimer>
    <platform>all</platform>
  </extension>
</addon>
