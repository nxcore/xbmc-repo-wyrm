<includes>
	<include name="IconView">
		<control type="panel" id="52">
			<posx>20</posx>
			<posy>100</posy>
			<width>680</width>
			<height>394</height>
			<onleft>2</onleft>
			<onright>29</onright>
			<onup>52</onup>
			<ondown>52</ondown>
			<include>list-fade</include>
			<viewtype label="31944">Icons</viewtype>
			<scrolltime>200</scrolltime>
			<pagecontrol>29</pagecontrol>
			<itemlayout height="130" width="130">
				<control type="image">
					<width>100</width>
					<height>100</height>
					<posx>15</posx>
					<posy>15</posy>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>20</posx>
					<posy>10</posy>
					<width>90</width>
					<height>90</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>75</posx>
					<posy>75</posy>
					<width>28</width>
					<height>28</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="label">
					<posx>60</posx>
					<posy>105</posy>
					<width>120</width>
					<height>22</height>
					<font>font11</font>
					<textcolor>button-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="130" width="130">
				<control type="image">
					<width>100</width>
					<height>100</height>
					<posx>15</posx>
					<posy>15</posy>
					<texture>folder-focus.png</texture>
					<animation effect="zoom" end="0,0,110,110" time="100">focus</animation>
				</control>
				<control type="image">
					<posx>20</posx>
					<posy>10</posy>
					<width>90</width>
					<height>90</height>
					<info>ListItem.Icon</info>
					<animation effect="zoom" end="5,5,100,100" time="100">focus</animation>
				</control>
				<control type="image">
					<posx>80</posx>
					<posy>80</posy>
					<width>28</width>
					<height>28</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="image">
					<width>100</width>
					<height>100</height>
					<posx>15</posx>
					<posy>15</posy>
					<texture>folder-focus2.png</texture>
					<animation effect="zoom" end="0,0,110,110" time="100">focus</animation>
				</control>
				<control type="label">
					<posx>60</posx>
					<posy>108</posy>
					<width>120</width>
					<height>22</height>
					<font>font11</font>
					<textcolor>button-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="scrollbar" id="29">
			<posx>700</posx>
			<posy>82</posy>
			<width>14</width>
			<height>422</height>
			<texturesliderbackground border="0,8,0,8">scrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="0,8,0,8">scrollbar_bar.png</texturesliderbar>
			<texturesliderbarfocus border="0,8,0,8">scrollbar_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<onleft>52</onleft>
			<onright>2</onright>
			<include>list-fade</include>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<colordiffuse>ScrollBarColor</colordiffuse>
			<visible>Skin.HasSetting(sbar) + Control.IsVisible(52)</visible>
		</control>
	</include>
</includes>
