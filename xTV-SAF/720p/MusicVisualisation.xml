<window id="2006">
	<allowoverlay>no</allowoverlay>
	<visible>!Window.IsVisible(2004)</visible>
	<visible>!Window.IsVisible(2005)</visible>
	<controls>
		<control type="image">
			<description>solid background</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<colordiffuse>solid-background</colordiffuse>
			<texture>background.png</texture>
			<visible>!Skin.String(custom-background)</visible>
		</control>
		<control type="image">
			<description>custom background</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<info>Skin.String(custom-background)</info>
			<colordiffuse>osd-background</colordiffuse>
			<visible>Skin.String(custom-background)</visible>
		</control>
		<control type="visualisation" id="2">
			<description>visualisation</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<visible>Skin.HasSetting(np-vis) + ![StringCompare(Visualisation.Name,milkdrop) | StringCompare(Visualisation.Name,projectm)]</visible>
		</control>
		<control type="group">
			<visible>!Skin.HasSetting(np-vis)</visible>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<texture background="true">$INFO[MusicPlayer.Property(Fanart_Image)]</texture>
				<aspectratio>scale</aspectratio>
				<fadetime>1000</fadetime>
			</control>
		</control>
		<control type="group">
			<visible>!Skin.HasSetting(np-vis)</visible>
			<animation effect="fade" time="300">VisibleChange</animation>
			<!-- infos -->
			<control type="group">
				<visible>Player.HasMedia + Player.ShowInfo</visible>
				<control type="image" id="992">
					<description>Music logo if available</description>
					<posx>8</posx>
					<posy>20</posy>
					<width>400</width>
					<height>155</height>
					<texture fallback="blank.png">$INFO[Player.FolderPath,,logo.png]</texture>
					<aligny>bottom</aligny>
					<aspectratio align="left">keep</aspectratio>
				</control>
				<control type="image" id="993">
					<description>Otherwise try parent directory of above</description>
					<posx>8</posx>
					<posy>20</posy>
					<width>400</width>
					<height>155</height>
					<texture fallback="blank.png">$INFO[Player.FolderPath,,../logo.png]</texture>
					<aligny>bottom</aligny>
					<aspectratio align="left">keep</aspectratio>
					<visible>StringCompare(Control.GetLabel(992),blank.png)</visible>
				</control>
				<control type="image">
					<description>media info background image</description>
					<posx>0</posx>
					<posy>565</posy>
					<width>1280</width>
					<height>155</height>
					<texture>background.png</texture>
					<colordiffuse>osd-background</colordiffuse>
				</control>
				<control type="image">
					<description>cover image</description>
					<posx>20</posx>
					<posy>470</posy>
					<width>225</width>
					<height>225</height>
					<info>MusicPlayer.Cover</info>
					<aspectratio align="bottom">keep</aspectratio>
				</control>
				<control type="label">
					<description>Title label</description>
					<posx>265</posx>
					<posy>560</posy>
					<width>761</width>
					<height>45</height>
					<info>MusicPlayer.Title</info>
					<align>left</align>
					<font>font18</font>
					<scroll>true</scroll>
				</control>
				<control type="label">
					<description>Artist label</description>
					<posx>265</posx>
					<posy>605</posy>
					<width>787</width>
					<height>37</height>
					<info>MusicPlayer.Artist</info>
					<align>left</align>
					<font>font12</font>
				</control>
				<control type="label">
					<description>Album label</description>
					<posx>265</posx>
					<posy>635</posy>
					<width>761</width>
					<height>37</height>
					<info>MusicPlayer.Album</info>
					<align>left</align>
					<font>font12</font>
				</control>
				<control type="label">
					<description>time label</description>
					<posx>265</posx>
					<posy>665</posy>
					<width>761</width>
					<height>37</height>
					<label>$INFO[musicplayer.Time]$INFO[musicplayer.Duration, / ]</label>
					<align>left</align>
					<font>font12</font>
				</control>
				<control type="label">
					<description>Playlist</description>
					<posx>1239</posx>
					<posy>580</posy>
					<width>265</width>
					<height>37</height>
					<align>right</align>
					<label>$INFO[musicplayer.Playlistposition,Track ]$INFO[musicplayer.Playlistlength, / ]</label>
					<font>font12</font>
				</control>
				<control type="label">
					<description>PartyMode Enabled</description>
					<posx>1239</posx>
					<posy>680</posy>
					<width>354</width>
					<height>37</height>
					<label>589</label>
					<visible>MusicPartyMode.Enabled</visible>
					<align>right</align>
					<font>font12</font>
				</control>
				<control type="progress">
					<description>Progressbar</description>
					<posx>454</posx>
					<posy>676</posy>
					<width>790</width>
					<height>16</height>
					<info>Player.Progress</info>
					<texturebg border="14,0,14,0">progress-background.png</texturebg>
					<midtexture border="14,0,14,0">progress-middle.png</midtexture>
					<colordiffuse>ProgressBarColor</colordiffuse>
				</control>
				<control type="slider">
					<description>Seek Slider</description>
					<posx>450</posx>
					<posy>674</posy>
					<width>790</width>
					<height>20</height>
					<texturesliderbar border="8,0,8,0">seekslider.png</texturesliderbar>
					<textureslidernib>progress-marker.png</textureslidernib>
					<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
					<action>seek</action>
				</control>
			</control>
		</control>
		<!-- "Now Playing" Stuff -->
		<control type="group">
			<visible>Skin.HasSetting(np-vis)</visible>
			<!-- Start Tilted Album Cover -->
			<control type="group">
				<visible>Skin.HasSetting(tilted-icons)</visible>
				<control type="image">
					<description>Album Cover</description>
					<posx>840</posx>
					<posy>310</posy>
					<width>320</width>
					<height>320</height>
					<info>MusicPlayer.Cover</info>
					<animation effect="rotatey" start="20" end="20" time="0" condition="Skin.HasSetting(tilted-icons)">Conditional</animation>
				</control>
				<control type="image">
					<description>Cover reflection</description>
					<posx>840</posx>
					<posy>630</posy>
					<width>320</width>
					<height>320</height>
					<texture flipy="true" diffuse="mask-fade.png">-</texture>
					<info>MusicPlayer.Cover</info>
					<animation effect="rotatey" start="20" end="20" time="0" condition="Skin.HasSetting(tilted-icons)">Conditional</animation>
				</control>
			</control>
			<!-- Start Non tilted Album Cover -->
			<control type="group">
				<visible>!Skin.HasSetting(tilted-icons)</visible>
				<control type="image">
					<description>Album Cover</description>
					<posx>840</posx>
					<posy>310</posy>
					<width>320</width>
					<height>320</height>
					<info>MusicPlayer.Cover</info>
				</control>
				<control type="image">
					<description>Cover reflection</description>
					<posx>840</posx>
					<posy>630</posy>
					<width>320</width>
					<height>320</height>
					<texture flipy="true" diffuse="mask-fade.png">-</texture>
					<info>MusicPlayer.Cover</info>
				</control>
			</control>
			<control type="label">
				<description>Title label</description>
				<posx>740</posx>
				<posy>463</posy>
				<width>637</width>
				<height>40</height>
				<info>MusicPlayer.Title</info>
				<align>right</align>
				<font>font18</font>
				<scroll>true</scroll>
			</control>
			<control type="label">
				<description>Artist label</description>
				<posx>740</posx>
				<posy>513</posy>
				<width>637</width>
				<height>25</height>
				<info>MusicPlayer.Artist</info>
				<align>right</align>
				<font>font12</font>
				<scroll>true</scroll>
			</control>
			<control type="label">
				<description>Album label</description>
				<posx>740</posx>
				<posy>551</posy>
				<width>637</width>
				<height>25</height>
				<info>MusicPlayer.Album</info>
				<align>right</align>
				<font>font12</font>
				<scroll>true</scroll>
			</control>
			<control type="label">
				<description>Time label</description>
				<posx>740</posx>
				<posy>597</posy>
				<width>194</width>
				<height>25</height>
				<label>$INFO[MusicPlayer.Time]$INFO[MusicPlayer.Duration, / ]</label>
				<align>right</align>
				<font>font12</font>
			</control>
			<control type="progress">
				<description>Progressbar</description>
				<posx>100</posx>
				<posy>610</posy>
				<width>440</width>
				<height>16</height>
				<info>Player.Progress</info>
				<texturebg border="14,0,14,0">progress-background.png</texturebg>
				<midtexture border="14,0,14,0">progress-middle.png</midtexture>
				<colordiffuse>ProgressBarColor</colordiffuse>
			</control>
			<control type="slider">
				<description>Seek Slider</description>
				<posx>100</posx>
				<posy>608</posy>
				<width>440</width>
				<height>20</height>
				<texturesliderbar border="8,0,8,0">seekslider.png</texturesliderbar>
				<textureslidernib>progress-marker.png</textureslidernib>
				<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
				<action>seek</action>
			</control>
		</control>
		<!-- Codec -->
		<control type="group">
			<visible>Player.ShowCodec</visible>
			<control type="image">
				<description>preset background image</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>90</height>
				<texture>background.png</texture>
				<colordiffuse>osd-background</colordiffuse>
			</control>
			<control type="label">
				<description>Codecinfo</description>
				<posx>132</posx>
				<posy>12</posy>
				<width>1062</width>
				<height>37</height>
				<label>Audio | $INFO[musicplayer.Codec,Codec:,]$INFO[musicplayer.Bitrate, - Bitrate:,kbps]$INFO[musicplayer.bitspersample, - Bits:,bit]$INFO[musicplayer.Samplerate, - Freq.: ,kHz]$INFO[musicplayer.Channels, - Channels: ]</label>
				<align>left</align>
				<font>font11</font>
			</control>
			<control type="label">
				<description>Presets label</description>
				<posx>132</posx>
				<posy>43</posy>
				<width>991</width>
				<height>37</height>
				<label>Visual | fps: $INFO[System.FPS] -</label>
				<font>font11</font>
				<align>left</align>
			</control>
			<control type="label">
				<description>Presets label</description>
				<posx>398</posx>
				<posy>43</posy>
				<width>743</width>
				<height>37</height>
				<label>$INFO[Visualisation.Name]$INFO[Visualisation.Preset, - ]</label>
				<font>font11</font>
				<align>left</align>
			</control>
			<control type="label">
				<description>Preset locked image</description>
				<posx>371</posx>
				<posy>45</posy>
				<width>21</width>
				<height>18</height>
				<visible>Visualisation.Locked</visible>
				<texture>arenaitem-private.png</texture>
			</control>
		</control>
	</controls>
</window>