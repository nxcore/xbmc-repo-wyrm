<includes>
	<include name="PhotoWrap">
		<control type="wraplist" id="54">
			<viewtype label="31942">Wraplist</viewtype>
			<posx>10</posx>
			<posy>193</posy>
			<width>1256</width>
			<height>500</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>9000</onup>
			<ondown>1354</ondown>
			<focusposition>2</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>5</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>1354</pagecontrol>
			<itemlayout height="202" width="202">
				<control type="image">
					<posx>16</posx>
					<posy>232</posy>
					<width>175</width>
					<height>180</height>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<fadetime>300</fadetime>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
				<control type="image">
					<posx>16</posx>
					<posy>232</posy>
					<width>175</width>
					<height>180</height>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<bordertexture border="6">picturepanel.png</bordertexture>
					<bordersize>6</bordersize>
					<fadetime>300</fadetime>
					<visible>!SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="420" width="428">
				<control type="image">
					<posx>14</posx>
					<posy>0</posy>
					<height>412</height>
					<width>403</width>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<bordertexture border="6">picturepanel.png</bordertexture>
					<bordersize>6</bordersize>
					<fadetime>300</fadetime>
					<visible>!SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
				<control type="image">
					<posx>14</posx>
					<posy>0</posy>
					<height>412</height>
					<width>403</width>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<fadetime>300</fadetime>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<!-- Scroll Bar -->
			<control type="scrollbar" id="1354">
				<posx>17</posx>
				<posy>612</posy>
				<width>1239</width>
				<height>22</height>
				<onup>54</onup>
				<ondown>9000</ondown>
				<orientation>horizontal</orientation>
				<visible>Skin.HasSetting(sbar) + SubString(Container.Viewmode,Wrap,Left)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>17</posx>
				<posy>633</posy>
				<width>1239</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Label -->
			<control type="label">
				<posx>17</posx>
				<posy>633</posy>
				<width>1239</width>
				<height>37</height>
				<font>font22</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Resolution -->
			<control type="label">
				<posx>283</posx>
				<posy>683</posy>
				<width>708</width>
				<height>37</height>
				<font>font13</font>
				<align>center</align>
				<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
				<info>ListItem.PictureResolution</info>
				<textoffsetx>0</textoffsetx>
			</control>
		</control>
	</include>
</includes>
