<window id="115">
	<defaultcontrol always="true">1</defaultcontrol>
	<visible>Player.Seeking | Player.DisplayAfterSeek | Window.IsActive(VideoOSD) | Window.IsActive(MusicOSD) | Player.Paused | Player.Forwarding | Player.Rewinding </visible>
	<animation effect="fade" start="0" time="100">WindowOpen</animation>
	<animation effect="fade" end="0" time="200">WindowClose</animation>
	<coordinates>
		<posx>0</posx>
		<posy>640</posy>
	</coordinates>
	<controls>
		<control type="group">
			<visible>![Window.IsVisible(SliderDialog) | Window.IsVisible(OSDVideoSettings) | Window.IsVisible(OSDAudioSettings) | Window.IsVisible(VideoBookmarks) | Window.IsVisible(script.pseudotv.TVOverlay.xml)]</visible>
			<control type="image">
				<description>Seekbar Dialog background image</description>
				<posx>-42</posx>
				<posy>6</posy>
				<height>80</height>
				<width>1359</width>
				<texture>background.png</texture>
				<colordiffuse>osd-background</colordiffuse>
				<visible>[Window.IsActive(2005)] + ![Window.IsActive(MusicOSD)] + ![Window.IsVisible(fullscreeninfo)]</visible>
			</control>
			<!-- Media Icon -->
			<control type="group">
				<visible>!Window.IsVisible(fullscreeninfo)</visible>
				<control type="group">
				<!-- tvshows -->
					<visible>VideoPlayer.Content(episodes) + Window.IsVisible(FullscreenVideo)</visible>
					<animation effect="fade" time="200">VisibleChange</animation>
					<control type="image" id="990">
						<description>Use TV Clearart if available(episodes in season folders)</description>
						<posx>8</posx>
						<posy>-145</posy> 
						<width>275</width>
						<height>145</height>
						<texture fallback="blank.png">$INFO[Player.FolderPath,,../clearart.png]</texture>
						<aspectratio>keep</aspectratio>
					</control>
					<control type="image" id="991">
						<description>Use TV Clearart if available(episodes in tv show folder)</description>
						<posx>8</posx>
						<posy>-145</posy> 
						<width>275</width>
						<height>145</height>
						<texture fallback="blank.png">$INFO[Player.FolderPath,,clearart.png]</texture>
						<aspectratio>keep</aspectratio>
						<visible>StringCompare(Control.GetLabel(990),blank.png)</visible>
					</control>
					<control type="group">
						<visible>StringCompare(Control.GetLabel(990),blank.png) + StringCompare(Control.GetLabel(991),blank.png)</visible>
						<control type="image" id="992">
							<description>Otherwise try TV logo if available(episodes in season folders)</description>
							<posx>8</posx>
							<posy>-75</posy>
							<width>276</width>
							<height>75</height>
							<texture fallback="blank.png">$INFO[Player.FolderPath,,logo.png]</texture>
							<aligny>bottom</aligny>
							<aspectratio align="left">keep</aspectratio>
						</control>
						<control type="image" id="993">
							<description>Otherwise try TV logo if available(episodes in tv show folder)</description>
							<posx>8</posx>
							<posy>-75</posy>
							<width>276</width>
							<height>75</height>
							<texture fallback="blank.png">$INFO[Player.FolderPath,,../logo.png]</texture>
							<aligny>bottom</aligny>
							<aspectratio align="left">keep</aspectratio>
							<visible>StringCompare(Control.GetLabel(992),blank.png)</visible>
						</control>
						<control type="group">
							<visible>StringCompare(Control.GetLabel(992),blank.png) + StringCompare(Control.GetLabel(993),blank.png)</visible>
							<control type="image" id="994">
								<description>Otherwise try TV folder.jpg if available(episodes in season folders)</description>
								<posx>80</posx>
								<posy>-180</posy>
								<width>132</width>
								<height>180</height>
								<texture fallback="blank.png" diffuse="mask-rect.png">$INFO[Player.FolderPath,,folder.jpg]</texture>
								<aligny>bottom</aligny>
								<aspectratio align="left">keep</aspectratio>
							</control>
							<control type="image" id="995">
								<description>Otherwise try TV folder.jpg if available(episodes in tv show folder)</description>
								<posx>80</posx>
								<posy>-180</posy>
								<width>132</width>
								<height>180</height>
								<texture fallback="blank.png" diffuse="mask-rect.png">$INFO[Player.FolderPath,,../folder.jpg]</texture>
								<aligny>bottom</aligny>
								<aspectratio align="left">keep</aspectratio>
								<visible>StringCompare(Control.GetLabel(994),blank.png)</visible>
							</control>
						</control>
					</control>
				</control>
				<!-- movies -->
				<control type="group">
					<visible>VideoPlayer.Content(Movies) + Window.IsVisible(FullscreenVideo)</visible>
					<animation effect="fade" time="200">VisibleChange</animation>
					<!-- tvshows -->
					<control type="image" id="890">
						<description>Use Movie Clearart if available</description>
						<posx>8</posx>
						<posy>-145</posy> 
						<width>275</width>
						<height>145</height>
						<texture fallback="blank.png">$INFO[Player.FolderPath,,clearart.png]</texture>
						<aspectratio>keep</aspectratio>
					</control>
					<control type="group">
						<visible>StringCompare(Control.GetLabel(890),blank.png)</visible>
						<control type="image" id="891">
							<description>Otherwise try Movie logo if available</description>
							<posx>8</posx>
							<posy>-75</posy>
							<width>276</width>
							<height>75</height>
							<texture fallback="blank.png">$INFO[Player.FolderPath,,logo.png]</texture>
							<aligny>bottom</aligny>
							<aspectratio align="left">keep</aspectratio>
							<visible>StringCompare(Control.GetLabel(890),blank.png)</visible>
						</control>
						<control type="group">
							<visible>StringCompare(Control.GetLabel(891),blank.png)</visible>
							<control type="image" id="892">
								<description>Otherwise try TV folder.jpg if available(episodes in season folders)</description>
								<posx>80</posx>
								<posy>-180</posy>
								<width>132</width>
								<height>180</height>
								<texture fallback="blank.png" diffuse="mask-rect.png">$INFO[Player.FolderPath,,folder.jpg]</texture>
								<aligny>bottom</aligny>
								<aspectratio align="left">keep</aspectratio>
							</control>
							<control type="image" id="893">
								<description>Fallback to video cover image if no clearart or logo</description>
								<posx>80</posx>
								<posy>-180</posy>
								<width>132</width>
								<height>180</height>
								<aligny>bottom</aligny>
								<texture diffuse="mask-rect.png">$INFO[VideoPlayer.cover]</texture>
								<aspectratio align="left">keep</aspectratio>
								<visible>StringCompare(Control.GetLabel(892),blank.png)</visible>
							</control>
						</control>
					</control>
				</control>
			</control>	
			<!-- music -->	
			<control type="image">
				<description>Music cover image</description>
				<posx>8</posx>
				<posy>-256</posy>
				<width>250</width>
				<height>250</height>
				<align>left</align>
				<aligny>bottom</aligny>
				<texture diffuse="mask-rect.png">$INFO[MusicPlayer.Cover]</texture>
				<aspectratio align="left">keep</aspectratio>
				<animation effect="fade" time="200">VisibleChange</animation>
				<visible>Player.HasAudio + Window.IsVisible(Visualisation) + !Window.IsVisible(MusicOSD) + !Player.ShowInfo</visible>
			</control>
		</control>	
		<!-- End of Media Icon -->
		
		<!-- Player action labels -->
		<control type="group">
			<visible>[Window.IsVisible(FullscreenVideo) | Window.IsVisible(Visualisation)]</visible>
			<control type="label">
				<description>Playback State Label</description>
				<posx>424</posx>
				<posy>30</posy>
				<height>38</height>
				<width>424</width>
				<align>center</align>
				<aligny>center</aligny>
				<font>font22</font>
				<label>$VAR[PlaybackState]</label>
			</control>
		</control>
		<!-- End Player action labels -->
		
		<control type="group">
			<visible>[Window.IsVisible(FullscreenVideo)]</visible>
			<visible>![Window.IsVisible(SliderDialog) | Window.IsVisible(OSDVideoSettings) | Window.IsVisible(OSDAudioSettings) | Window.IsVisible(VideoBookmarks) | Window.IsVisible(script.pseudotv.TVOverlay.xml)]</visible>
			<!-- Player info labels -->
			<control type="label">
				<posx>53</posx>
				<posy>5</posy>
				<width>354</width>
				<height>31</height>
				<font>font13</font>
				<label>$INFO[VideoPlayer.Time]</label>
			</control>
			<control type="label">
				<posx>53</posx>
				<posy>35</posy>
				<width>424</width>
				<height>31</height>
				<font>font13</font>
				<label>$LOCALIZE[31128] $INFO[System.Time]</label>
			</control>
			<control type="label">
				<posx>1239</posx>
				<posy>5</posy>
				<width>354</width>
				<height>31</height>
				<align>right</align>
				<font>font13</font>
				<label>$INFO[VideoPlayer.Duration]</label>
			</control>
			<control type="label">
				<posx>1239</posx>
				<posy>35</posy>
				<width>424</width>
				<height>31</height>
				<align>right</align>
				<font>font13</font>
				<label>$LOCALIZE[31129] $INFO[Player.FinishTime]</label>
			</control>
			<!-- End Player info labels -->
			<control type="group">
				<visible>!Window.IsVisible(fullscreeninfo)</visible>
				<!-- Progress bar -->
				<control type="progress">
					<description>Progressbar Cache</description>
					<posx>214</posx>
					<posy>12</posy>
					<width>864</width>
					<height>16</height>
					<info>Player.ProgressCache</info>
					<midtexture border="14,0,14,0">progress-middle.png</midtexture>
					<colordiffuse>FF777777</colordiffuse>
				</control>
				<control type="progress">
					<description>Progressbar</description>
					<posx>214</posx>
					<posy>12</posy>
					<width>864</width>
					<height>16</height>
					<info>Player.Progress</info>
					<texturebg border="14,0,14,0">progress-background.png</texturebg>
					<midtexture border="14,0,14,0">progress-middle.png</midtexture>
					<colordiffuse>ProgressBarColor</colordiffuse>
				</control>
				<!-- End Progress Bar -->
				<!-- Seek Slider -->
				<control type="slider" id="401">
					<description>Seek Slider</description>
					<posx>214</posx>
					<posy>10</posy>
					<width>864</width>
					<height>20</height>
					<texturesliderbar border="8,0,8,0">seekslider.png</texturesliderbar>
					<textureslidernib>progress-marker.png</textureslidernib>
					<textureslidernibfocus>progress-marker.png</textureslidernibfocus>
					<action>seek</action>
				</control>
				<!-- End Seek Slider -->
			</control>	
		</control>
		<!-- Start Progress Control for all Windows Except VideoFullScreen , KaraokeVisualisation, MusicVisualisation, MyMusicPlaylist and PlayerControls-->
		<control type="group">
			<visible>![Window.IsActive(FullscreenVideo) | Window.IsActive(karaoke) | Window.IsActive(Visualisation) | Window.IsActive(musicplaylist) | Window.IsActive(PlayerControls)]</visible>
			<posx>463</posx>
			<posy>-620</posy>
			<control type="image">
				<description>Popup Volume Dialog Background</description>
				<posx>0</posx>
				<posy>2</posy>
				<width>354</width>
				<height>87</height>
				<texture border="8" diffuse="mask-squ.png">button-menu-background.png</texture>
			</control>
			<control type="label">
				<description>Elapsed Time Label</description>
				<posx>146</posx>
				<posy>25</posy>
				<font>font12</font>
				<align>right</align>
				<info>Player.Time</info>
				<width>177</width>
				<height>20</height>
				<visible>!Player.Seeking</visible>
			</control>
			<control type="label">
				<description>Seek Time Label</description>
				<posx>146</posx>
				<posy>25</posy>
				<font>font12</font>
				<align>right</align>
				<info>Player.SeekTime</info>
				<width>177</width>
				<height>20</height>
				<visible>Player.Seeking</visible>
			</control>
			<control type="label">
				<description>Duration Label</description>
				<posx>156</posx>
				<posy>25</posy>
				<label>$INFO[Player.Duration,/ ]</label>
				<font>font12</font>
				<align>left</align>
				<width>177</width>
				<height>20</height>
			</control>
			<control type="progress">
				<description>Progressbar Cache</description>
				<posx>18</posx>
				<posy>66</posy>
				<width>300</width>
				<height>8</height>
				<info>Player.ProgressCache</info>
				<midtexture border="14,0,14,0">progress-middle.png</midtexture>
				<colordiffuse>FF777777</colordiffuse>
			</control>
			<control type="progress">
				<description>Progressbar</description>
				<posx>18</posx>
				<posy>66</posy>
				<width>300</width>
				<height>8</height>
				<info>Player.Progress</info>
				<colordiffuse>ProgressBarColor</colordiffuse>
				<texturebg border="14,0,14,0">progress-background.png</texturebg>
				<midtexture border="14,0,14,0">progress-middle.png</midtexture>
			</control>
		</control>
	</controls>
</window>