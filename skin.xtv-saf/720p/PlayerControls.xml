				<font>font12</font>
				<textcolor>listselected-text</textcolor>
			</control>
			<control type="label">
				<posx>354</posx>
				<posy>168</posy>
				<width>778</width>
				<height>31</height>
				<label>$INFO[VideoPlayer.Title]</label>
				<font>font12</font>
				<align>center</align>
			</control>
			<control type="label">
				<posx>540</posx>
				<posy>205</posy>
				<width>170</width>
				<height>31</height>
				<font>font11</font>
				<label>$INFO[VideoPlayer.Time]</label>
