		<!-- End of Media Info-->
		<!-- Player buttons -->
		<control type="grouplist" id="999">
			<description>Group OSD Button</description>
			<posx>354</posx>
			<posy>245</posy>
			<width>778</width>
			<height>40</height>
			<itemgap>3</itemgap>
			<scrolltime>200</scrolltime>
			<orientation>horizontal</orientation>
			<align>center</align>
			<usecontrolcoords>false</usecontrolcoords>
			<onleft>999</onleft>
			<onright>999</onright>
			<onup>999</onup>
			<ondown>999</ondown>
			<visible>true</visible>
			<control type="button" id="208">
				<description>DVD Menu</description>
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-dvdmenu-focus.png</texturefocus>
				<texturenofocus>osd-btn-dvdmenu.png</texturenofocus>
				<onclick>XBMC.PlayerControl(ShowVideoMenu)</onclick>
				<visible>VideoPlayer.HasMenu</visible>
			</control>
			<control type="button" id="950">
				<description>Subtitles Menu</description>
				<width>40</width>
				<height>40</height>
				<texturenofocus>osd-btn-subtitle.png</texturenofocus>
				<texturefocus>osd-btn-subtitle-focus.png</texturefocus>
				<onclick>Dialog.Close(VideoOSD)</onclick>
				<onclick>XBMC.RunScript($INFO[Skin.String(osd-subscriptpath)])</onclick>
				<visible>Player.HasVideo + !IsEmpty(Skin.String(osd-subscriptpath))</visible>
			</control>
			<control type="button" id="951">
				<description>Lyrics Menu</description>
				<width>40</width>
				<height>40</height>
				<texturenofocus>osd-btn-lyric.png</texturenofocus>
				<texturefocus>osd-btn-lyric-focus.png</texturefocus>
				<onclick>Dialog.Close(playercontrols)</onclick>
				<onclick>XBMC.RunScript($INFO[Skin.String(osd-lyricpath)])</onclick>
				<visible>Player.HasAudio + !IsEmpty(Skin.String(osd-lyricpath))</visible>
			</control>
			<control type="togglebutton" id="908">
				<width>40</width>
				<height>40</height>
				<texturefocus>osd-btn-playthru-focus.png</texturefocus>
				<texturenofocus>osd-btn-playthru.png</texturenofocus>
				<usealttexture>Playlist.IsRandom</usealttexture>
				<alttexturefocus>osd-btn-random-focus.png</alttexturefocus>
				<alttexturenofocus>osd-btn-random.png</alttexturenofocus>
				<onclick>XBMC.PlayerControl(Random)</onclick>
				<visible>Player.HasAudio</visible>
			</control>
			<control type="group" id="925">
				<width>40</width>
				<height>40</height>
				<visible>Player.HasAudio</visible>
				<control type="button" id="912">
					<width>40</width>
					<height>40</height>
					<onleft>908</onleft>
					<onright>217</onright>
					<texturefocus>-</texturefocus>
					<texturenofocus>-</texturenofocus>
					<onclick>PlayerControl(Repeat)</onclick>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeat.png</texture>
					<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
					<visible>!Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeat-focus.png</texture>
					<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
					<visible>Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatone.png</texture>
					<visible>Playlist.IsRepeatOne</visible>
					<visible>!Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatone-focus.png</texture>
					<visible>Playlist.IsRepeatOne</visible>
					<visible>Control.HasFocus(912)</visible>
				</control>
				<control type="image">
					<width>40</width>
					<height>40</height>
					<texture>osd-btn-repeatall.png</texture>
					<visible>Playlist.IsRepeat</visible>
