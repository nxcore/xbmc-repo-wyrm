				<posx>520</posx>
				<posy>223</posy>
				<width>440</width>
				<height>16</height>
				<info>Player.Progress</info>
				<colordiffuse>ProgressBarColor</colordiffuse>
			</control>
			<control type="slider">
				<posx>520</posx>
				<posy>221</posy>
				<width>440</width>
				<height>20</height>
				<action>seek</action>
			</control>
			<control type="label">
				<posx>970</posx>
				<posy>216</posy>
				<label>- $INFO[MusicPlayer.TimeRemaining]</label>
				<font>font12</font>
				<width>220</width>
				<height>20</height>
			</control>
