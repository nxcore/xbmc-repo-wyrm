			</control>
			<control type="label">
				<posx>940</posx>
				<posy>205</posy>
				<width>170</width>
				<height>31</height>
				<font>font11</font>
				<label>- $INFO[VideoPlayer.TimeRemaining]</label>
			</control>
		</control>
		<!-- Music -->
 		<control type="group">
			<visible>Player.HasAudio</visible>
			<control type="image">
				<posx>132</posx>
				<posy>137</posy>
				<width>212</width>
				<height>150</height>
				<align>left</align>
				<aligny>bottom</aligny>
				<fadetime>300</fadetime>
				<texture>$INFO[MusicPlayer.Cover]</texture>
				<aspectratio>keep</aspectratio>
				<visible>!Window.IsActive(visualisation)</visible>
			</control>
			<control type="label">
				<posx>354</posx>
				<posy>132</posy>
				<width>778</width>
				<height>20</height>
				<label>$INFO[MusicPlayer.Artist]</label>
				<font>font12</font>
				<align>center</align>
			</control>
			<control type="label">
				<posx>354</posx>
				<posy>160</posy>
				<width>778</width>
				<height>20</height>
				<label>$INFO[MusicPlayer.Album]</label>
