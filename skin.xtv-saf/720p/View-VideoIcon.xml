<includes>
	<include name="VideoIconView">
		<control type="panel" id="52">
			<posx>20</posx>
			<posy>88</posy>
			<width>1200</width>
			<height>584</height>
			<onleft>9000</onleft>
			<onright>29</onright>
			<onup>52</onup>
			<ondown>52</ondown>
			<include>list-fade</include>
			<viewtype label="31944">Icons</viewtype>
			<scrolltime>200</scrolltime>
			<pagecontrol>29</pagecontrol>
			<itemlayout height="194" width="300">
				<control type="image">
					<posx>100</posx>
					<posy>20</posy>
					<width>100</width>
					<height>140</height>
					<texture background="true" diffuse="mask-rect.png">$INFO[ListItem.Icon]</texture>
				</control>
				<control type="image">
					<posx>210</posx>
					<posy>10</posy>
					<width>32</width>
					<height>32</height>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>150</posx>
					<posy>170</posy>
					<width>280</width>
					<height>22</height>
					<font>font11</font>
					<align>center</align>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="194" width="300">
				<control type="image">
					<posx>100</posx>
					<posy>20</posy>
					<width>100</width>
					<height>140</height>
					<texture background="true" diffuse="mask-rect.png">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="100" end="116" center="auto" time="600" tween="back" easing="out" />
					</animation>
				</control>
				<control type="image">
					<posx>210</posx>
					<posy>10</posy>
					<width>32</width>
					<height>32</height>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>150</posx>
					<posy>170</posy>
					<width>280</width>
					<height>22</height>
					<font>font11</font>
					<align>center</align>
					<textcolor>listselectedtext</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="scrollbar" id="29">
			<posx>1239</posx>
			<posy>88</posy>
			<width>22</width>
			<height>588</height>
			<texturesliderbackground border="8">scrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="8">scrollbar_bar.png</texturesliderbar>
			<texturesliderbarfocus border="8">scrollbar_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<onleft>52</onleft>
			<onright>9000</onright>
			<include>list-fade</include>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<colordiffuse>ScrollBarColor</colordiffuse>
			<visible>Skin.HasSetting(sbar) + Control.IsVisible(52)</visible>
		</control>
	</include>
</includes>
