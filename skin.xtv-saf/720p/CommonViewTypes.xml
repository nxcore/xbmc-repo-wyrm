<includes>
	<include name="ListView">
		<control type="fixedlist" id="50">
			<viewtype label="31941">List</viewtype>
			<posx>678</posx>
			<posy>116</posy>
			<width>540</width>
			<height>550</height>
			<focusposition>4</focusposition>
			<movement>6</movement>
			<onleft>9000</onleft>
			<onright>23</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<include>list-fade</include>
			<scrolltime>200</scrolltime>
			<preloaditems>5</preloaditems>
			<pagecontrol>23</pagecontrol>
			<itemlayout width="560" height="50">
				<control type="image">
					<posx>10</posx>
					<posy>8</posy>
					<width>32</width>
					<height>32</height>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>53</posx>
					<posy>5</posy>
					<width>486</width>
					<height>26</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout width="560" height="50">
				<control type="image">
					<posx>10</posx>
					<posy>8</posy>
					<width>32</width>
					<height>32</height>
					<texture>$VAR[Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>53</posx>
					<posy>5</posy>
					<width>486</width>
					<height>26</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<posy>66</posy>
			<visible>Control.IsVisible(50)</visible>
			<visible>[IntegerGreaterThan(Container(50).NumItems,0) + !system.getbool(filelists.showparentdiritems)] | [IntegerGreaterThan(Container(50).NumItems,-1) + system.getbool(filelists.showparentdiritems)]</visible>
			<include>list-fade</include>
			<control type="group">
				<control type="image" description="Slider">
					<posx>716</posx>
					<posy>398</posy>
					<width>514</width>
					<height>50</height>
					<texture>default-button.png</texture>
					<aspectratio>stretch</aspectratio>
				</control>
				<animation condition="Container(50).Position(0)" effect="slide" start="0,0" end="0,-350"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(1)" effect="slide" start="0,0" end="0,-300"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(2)" effect="slide" start="0,0" end="0,-250"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(3)" effect="slide" start="0,0" end="0,-200"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(4)" effect="slide" start="0,0" end="0,-150"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(5)" effect="slide" start="0,0" end="0,-100"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(6)" effect="slide" start="0,0" end="0,-50"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(7)" effect="slide" start="0,0" end="0,0"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(8)" effect="slide" start="0,0" end="0,50"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(9)" effect="slide" start="0,0" end="0,100"
					time="0" easing="inout" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(10)" effect="slide" start="0,0" end="0,150"
					time="0" easing="inout" reversible="true">Conditional</animation>
			</control>
		</control>
		<control type="group">
			<visible>Control.IsVisible(50)</visible>
			<include>SortLetterNotification</include>
			<include>PageControl</include>
		</control>
	</include>
</includes>
