<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>23</defaultcontrol>
	<controls>
		<control type="group">
			<control type="group">
				<animation effect="fade" delay="400" start="0" end="100" time="200">WindowOpen</animation>
				<animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
				<control type="image">
					<description>solid background</description>
					<posx>0</posx>
					<posy>0</posy>
					<width>1280</width>
					<height>720</height>
					<colordiffuse>solid-background</colordiffuse>
					<texture>background.png</texture>
					<visible>IsEmpty(Skin.String(custom-background))</visible>
				</control>
				<control type="image">
					<description>custom background</description>
					<posx>0</posx>
					<posy>0</posy>
					<width>1280</width>
					<height>720</height>
					<colordiffuse>solid-background</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<texture>$INFO[Skin.String(custom-background)]</texture>
					<visible>!IsEmpty(Skin.String(custom-background))</visible>
				</control>
				<control type="label" id="1">
					<description>header label</description>
					<posx>35</posx>
					<posy>37</posy>
					<width>1203</width>
					<height>37</height>
					<font>font28</font>
					<align>center</align>
				</control>
				<control type="textbox" id="5">
					<description>textarea</description>
					<posx>35</posx>
					<posy>112</posy>
					<width>1185</width>
					<height>530</height>
					<label>-</label>
					<align>justify</align>
					<pagecontrol>23</pagecontrol>
					<autoscroll delay="3000" time="1000" repeat="6000">Skin.HasSetting(autoscroll)</autoscroll>
				</control>
				<control type="scrollbar" id="23">
					<posx>1244</posx>
					<posy>112</posy>
					<width>20</width>
					<height>530</height>
					<onleft>23</onleft>
					<onright>23</onright>
					<ondown>23</ondown>
					<onup>23</onup>
				</control>
			</control>
		</control>
	</controls>
</window>
