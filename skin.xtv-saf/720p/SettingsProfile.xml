<window>
	<defaultcontrol always="true">2</defaultcontrol>
	 
	<allowoverlay>yes</allowoverlay>
	<include>screen-fade</include>
	<controls>
		<include>background</include>
		<include>Fake-Overlay</include>
		<control type="image">
			<description>Settings Logo</description>
			<include>icon-header-commons</include>
			<texture>icon-settings.png</texture>
		</control>
		<control type="label">
			<description>Profiles Label</description>
			<posx>946</posx>
			<posy>50</posy>
			<width>309</width>
			<height>37</height>
			<label>31555</label>
			<font>font20</font>
		</control>

		<control type="list" id="2">
			<posx>716</posx>
			<posy>112</posy>
			<width>531</width>
			<height>450</height>
			<onleft>4</onleft>
			<onright>23</onright>
			<onup>2</onup>
			<ondown>2</ondown>
			<scrolltime>1</scrolltime>
			<viewtype label="31941">List</viewtype>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="56">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>504</width>
					<height>56</height>
				</control>
				<control type="label">
					<posx>21</posx>
					<posy>0</posy>
					<width>504</width>
					<height>56</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="56">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>504</width>
					<height>56</height>
					<texture border="8">default-button.png</texture>
					<visible>!Control.HasFocus(4)</visible>
				</control>
				<control type="label">
					<posx>21</posx>
					<posy>0</posy>
					<width>447</width>
					<height>56</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<selectedcolor>listselectedtext</selectedcolor>
					<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<!-- Scroll Bar -->
		<control type="scrollbar" id="23">
			<posx>1239</posx>
			<posy>112</posy>
			<width>22</width>
			<height>450</height>
			<onleft>2</onleft>
			<onright>4</onright>
			<visible>Skin.HasSetting(sbar)</visible>
		</control>
		<control type="image">
			<description>list item icon</description>
			<posx>214</posx>
			<posy>112</posy>
			<width>320</width>
			<height>320</height>
			<texture fallback="unknown-user.png" diffuse="mask-rect.png">$INFO[Container(2).ListItem.Icon]</texture>
			<aspectratio>keep</aspectratio>
		</control>
		<control type="radiobutton" id="4">
			<description>enable login screen</description>
			<posx>122</posx>
			<posy>440</posy>
			<width>504</width>
			<height>56</height>
			<onright>2</onright>
			<onleft>23</onleft>
			<onup>4</onup>
			<ondown>4</ondown>
			<label>20096</label>
			<radioposx>460</radioposx>
			<textoffsetx>20</textoffsetx>
		</control>

		<control type="fadelabel" id="3">
			<description>Last loaded</description>
			<posx>17</posx>
			<posy>650</posy>
			<width>1239</width>
			<height>40</height>
			<label>$LOCALIZE[13204] $INFO[system.profilename]</label>
			<font>font13</font>
			<align>center</align>
			<aligny>center</aligny>
		</control>
	</controls>
</window>