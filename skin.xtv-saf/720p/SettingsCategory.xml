<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>3</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<include>background</include>
		<include>Fake-Overlay</include>
		<control type="image"><!-- Settings logo -->
			<include>icon-header-commons</include>
			<texture>icon-settings.png</texture>
		</control>
		<control type="label" id="2"><!-- Settings screen heading -->
			<posx>946</posx>
			<posy>25</posy>
			<width>309</width>
			<height>31</height>
			<label>-</label>
			<font>font20</font>
		</control>
		<control type="label"><!-- Settings subscreen heading -->
			<posx>946</posx>
			<posy>62</posy>
			<width>309</width>
			<height>31</height>
			<label>$LOCALIZE[5]</label>
			<font>font20</font>
		</control>
		<control type="grouplist" id="3"><!-- Subheadings area -->
			<posx>940</posx>
			<posy>112</posy>
			<width>320</width>
			<height>434</height>
			<itemgap>-2</itemgap>
			<onleft>5</onleft>
			<onright>noop</onright>
			<onup>noop</onup>
			<ondown>20</ondown>
			<scrolltime tween="sine" easing="out">100</scrolltime>
		</control>

		<control type="button" id="20">
			<description>Setting level button</description>
			<posx>940</posx>
			<posy>598</posy>
			<width>320</width>
			<height>56</height>
			<font>font11</font>
			<textcolor>listselected-text</textcolor>
			<label>$LOCALIZE[10037]</label>
			<align>right</align>
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>3</onup>
			<ondown>noop</ondown>
			<onclick>SettingsLevelChange</onclick>
		</control>
		<control type="label"><!-- Settings level heading -->
			<posx>958</posx>
			<posy>598</posy>
			<width>320</width>
			<height>56</height>
			<label>$LOCALIZE[10004]:</label>
			<font>font11</font>
		</control>
		<!-- Markers for control area items (Don't show if scrollbar is in use)-->
		<control type="group">
			<visible>!Skin.HasSetting(sbar)</visible>
			<control type="image">
				<posx>484</posx>
				<posy>92</posy>
				<height>24</height>
				<width>24</width>
				<texture>scroll-up.png</texture>
				<visible>Container(5).HasPrevious</visible>
			</control>
			<control type="image">
				<posx>484</posx>
				<posy>648</posy>
				<height>24</height>
				<width>24</width>
				<texture>scroll-down.png</texture>
				<visible>Container(5).HasNext</visible>
			</control>
		</control>

		<control type="grouplist" id="5"><!-- Controls area -->
			<posx>53</posx>
			<posy>112</posy>
			<width>885</width>
			<height>542</height>
			<itemgap>-2</itemgap>
			<onleft>23</onleft>
			<onright>3</onright>
			<onup>noop</onup>
			<ondown>noop</ondown>
			<pagecontrol>23</pagecontrol>
			<scrolltime tween="sine" easing="out">100</scrolltime>
		</control>
		<control type="scrollbar" id="23"><!-- Scrollbar -->
			<posx>10</posx>
			<posy>112</posy>
			<width>18</width>
			<height>542</height>
			<onleft>9000</onleft>
			<onright>5</onright>
			<visible>Skin.HasSetting(sbar)</visible>
		</control>
		<control type="textbox" id="6"><!-- Settings description area -->
			<posx>10</posx>
			<posy>662</posy>
			<width>1260</width>
			<height>54</height>
			<font>rss20</font>
			<align>center</align>
			<include condition="Skin.HasSetting(autoscroll)">scrolltext</include>
		</control>
		<control type="button" id="7"><!-- Default button -->
			<posx>53</posx>
			<posy>187</posy>
			<width>870</width>
			<height>56</height>
			<textwidth>810</textwidth>
			<font>font12</font>
			<invalidcolor>invalid-text</invalidcolor>
		</control>
		<control type="radiobutton" id="8"><!-- Default radiobutton -->
			<posx>53</posx>
			<posy>187</posy>
			<width>870</width>
			<textwidth>810</textwidth>
			<height>56</height>
			<font>font12</font>
			<radioposx>844</radioposx>
		</control>
		<control type="spincontrolex" id="9"><!-- Default spincontrolex -->
			<posx>53</posx>
			<posy>187</posy>
			<width>870</width>
			<height>56</height>
			<font>font12</font>
			<reverse>yes</reverse>
		</control>
		<control type="button" id="10"><!-- Default category button -->
			<posx>53</posx>
			<posy>187</posy>
			<width>320</width>
			<height>56</height>
			<label>$LOCALIZE[224]</label>
			<font>font12</font>
		</control>
	</controls>
</window>