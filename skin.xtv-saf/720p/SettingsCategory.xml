			<onleft>5</onleft>
			<onright>23</onright>
			<onup>3</onup>
			<ondown>3</ondown>
		</control>
		
		<!-- Markers for control area items -->
		<control type="image">
			<posx>480</posx>
			<posy>88</posy>
			<height>27</height>
			<width>38</width>
			<texture>scroll-up.png</texture>
			<visible>Container(5).HasPrevious</visible>
		</control>
		<control type="image">
			<posx>480</posx>
			<posy>650</posy>
			<height>27</height>
			<width>38</width>
			<texture>scroll-down.png</texture>
			<visible>Container(5).HasNext</visible>
		</control>
		
		<control type="grouplist" id="5">
