			<itemgap>-2</itemgap>
			<onleft>23</onleft>
			<onright>3</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<pagecontrol>23</pagecontrol>
		</control>
		<!-- Scrollbar -->
		<control type="scrollbar" id="23">
			<posx>14</posx>
			<posy>112</posy>
			<width>22</width>
			<height>542</height>
			<onleft>3</onleft>
			<onright>5</onright>
			<include>list-fade</include>
			<visible>Skin.HasSetting(sbar) + [Container(5).HasPrevious | Container(5).HasNext]</visible>
		</control>
		<control type="button" id="7">
