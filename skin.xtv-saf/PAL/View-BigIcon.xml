<includes>
	<include name="BigIconView">
		<control type="panel" id="53">
			<posx>50</posx>
			<posy>120</posy>
			<width>700</width>
			<height>430</height>
			<onleft>2</onleft>
			<onright>23</onright>
			<onup>52</onup>
			<ondown>52</ondown>
			<viewtype label="Big Icons">Big Icons</viewtype>
			<scrolltime>200</scrolltime>
			<include>list-fade</include>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="220" width="200">
				<control type="image">
					<width>190</width>
					<height>190</height>
					<posx>5</posx>
					<posy>5</posy>
					<texture>-</texture>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>10</posy>
					<width>180</width>
					<height>180</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>145</posx>
					<posy>155</posy>
					<width>38</width>
					<height>38</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="label">
					<posx>95</posx>
					<posy>187</posy>
					<width>170</width>
					<height>22</height>
					<font>font12</font>
					<textcolor>button-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="230" width="230">
				<control type="image">
					<width>180</width>
					<height>180</height>
					<posx>5</posx>
					<posy>5</posy>
					<texture>folder-focus.png</texture>
					<animation effect="zoom" end="0,0,210,210" time="100">focus</animation>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>10</posy>
					<width>180</width>
					<height>180</height>
					<info>ListItem.Icon</info>
					<animation effect="zoom" end="5,5,200,200" time="100">focus</animation>
				</control>
				<control type="image">
					<width>180</width>
					<height>180</height>
					<posx>5</posx>
					<posy>5</posy>
					<texture>folder-focus2.png</texture>
					<animation effect="zoom" end="0,0,210,210" time="100">focus</animation>
				</control>
				<control type="image">
					<posx>165</posx>
					<posy>165</posy>
					<width>38</width>
					<height>38</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="label">
					<posx>105</posx>
					<posy>205</posy>
					<width>180</width>
					<height>22</height>
					<font>font12</font>
					<textcolor>button-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
	</include>
