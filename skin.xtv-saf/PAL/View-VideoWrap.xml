<includes>
	<include name="VideoWrapView">
		<control type="wraplist" id="54">
			<viewtype label="31943">Wraplist</viewtype>
			<posx>-45</posx>
			<posy>150</posy>
			<width>1000</width>
			<height>400</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>23</ondown>
			<include>list-fade</include>
			<focusposition>1</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>5</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="120" width="64">
				<control type="image">
					<posx>-36</posx>
					<posy>269</posy>
					<height>103</height>
					<width>56</width>
					<texture>case-dvd.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-35</posx>
					<posy>272</posy>
					<height>96</height>
					<width>52</width>
					<texture diffuse="mask-rect.png">DefaultVideo.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-35</posx>
					<posy>272</posy>
					<height>96</height>
					<width>52</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$INFO[ListItem.Icon]</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-35</posx>
					<posy>258</posy>
					<height>15</height>
					<width>15</width>
					<info>ListItem.Overlay</info>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="364" width="196">
				<control type="image">
					<posx>-14</posx>
					<posy>107</posy>
					<height>274</height>
					<width>170</width>
					<texture diffuse="mask-rect.png">hi-lite.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>115</posy>
					<height>257</height>
					<width>140</width>
					<texture>case-dvd.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>119</posy>
					<height>250</height>
					<width>136</width>
					<texture diffuse="mask-rect.png">DefaultVideo.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>119</posy>
					<height>250</height>
					<width>136</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$INFO[ListItem.Icon]</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>1</posx>
					<posy>82</posy>
					<height>39</height>
					<width>39</width>
					<info>ListItem.Overlay</info>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<!-- Star Rating -->
			<control type="image">
				<posx>175</posx>
				<posy>270</posy>
				<width>100</width>
				<height>20</height>
				<info>ListItem.StarRating</info>
				<colordiffuse>StarDiffuse</colordiffuse>
				<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				<visible>!IsEmpty(ListItem.StarRating)</visible>
			</control>
			<!-- MPAA Rating -->
			<control type="group">
				<posx>175</posx>
				<posy>300</posy>
				<include>mpaa-ratings</include>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>175</posx>
				<posy>345</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Title -->
			<control type="image" id="37016">
				<posx>175</posx>
				<posy>350</posy>
				<width>134</width>
				<height>52</height>
				<texture>$INFO[ListItem.path]logo.png</texture>
				<aligny>bottom</aligny>
				<aspectratio align="left">keep</aspectratio>
				<visible>container.content(tvshows)</visible>
			</control>
			<control type="image" id="39016">
				<posx>175</posx>
				<posy>350</posy>
				<width>134</width>
				<height>52</height>
				<texture>$INFO[ListItem.path]../logo.png</texture>
				<aligny>bottom</aligny>
				<aspectratio align="left">keep</aspectratio>
				<visible>container.content(tvshows)</visible>
			</control>
			<control type="label">
				<posx>175</posx>
				<posy>350</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Title</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
				<visible>[IsEmpty(Control.GetLabel(37016)) + IsEmpty(Control.GetLabel(39016))] | !container.content(tvshows)</visible>
				<animation effect="fade" delay="600" start="0" end="100" time="200">Visible</animation>
				<animation effect="fade" time="200">WindowClose</animation>
			</control>
			<!--<control type="label">
				<posx>175</posx>
				<posy>345</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>-->
			<!-- Genre -->
			<control type="label">
				<posx>175</posx>
				<posy>385</posy>
				<width>525</width>
				<height>30</height>
				<font>font13</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<selectedcolor>listselectedtext</selectedcolor>
				<scroll>true</scroll>
				<info>ListItem.Genre</info>
				<textoffsetx>0</textoffsetx>
			</control>
			<!-- Scroll Bar -->
			<control type="scrollbar" id="23">
				<posx>10</posx>
				<posy>527</posy>
				<width>700</width>
				<height>22</height>
				<texturesliderbackground border="10">scrollbar_background.png</texturesliderbackground>
				<texturesliderbar border="10">scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus border="10">scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<onup>54</onup>
				<ondown>2</ondown>
				<include>list-fade</include>
				<showonepage>false</showonepage>
				<orientation>horizontal</orientation>
				<colordiffuse>ScrollBarColor</colordiffuse>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
		</control>
	</include>
</includes>
