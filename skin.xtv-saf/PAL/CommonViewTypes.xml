<includes>
	<include name="ListView">
		<control type="fixedlist" id="50">
			<viewtype label="31941">List</viewtype>
			<posx>400</posx>
			<posy>95</posy>
			<width>300</width>
			<height>444</height>
			<focusposition>4</focusposition>
			<movement>6</movement>
			<onleft>2</onleft>
			<onright>23</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<include>list-fade</include>
			<scrolltime>1</scrolltime>
			<preloaditems>2</preloaditems>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="40">
				<control type="image">
					<posx>0</posx>
					<posy>4</posy>
					<width>24</width>
					<height>24</height>
					<texture>$INFO[ListItem.Overlay]</texture>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>-2</posy>
					<width>258</width>
					<height>32</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="40">
				<control type="image">
					<posx>22</posx>
					<posy>-3</posy>
					<width>270</width>
					<height>40</height>
					<texture>default-button.png</texture>
					<visible>!IntegerGreaterThan(Container(50).NumPages,1)</visible>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>4</posy>
					<width>24</width>
					<height>24</height>
					<info>ListItem.Overlay</info>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>-2</posy>
					<width>258</width>
					<height>32</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>button-text</textcolor>
					<shadowcolor>shadow-text</shadowcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="group" description="List furniture">
			<visible>Control.IsVisible(50) + IntegerGreaterThan(Container(50).NumPages,1)</visible>
			<control type="image" description="Slider">
				<posx>422</posx>
				<posy>132</posy>
				<width>274</width>
				<height>40</height>
				<texture>default-button.png</texture>
				<aspectratio>stretch</aspectratio>
				<animation condition="Container(50).Position(0)" effect="slide" start="0,0" end="0,-40"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(1)" effect="slide" start="0,0" end="0,0"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(2)" effect="slide" start="0,0" end="0,40"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(3)" effect="slide" start="0,0" end="0,80"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(4)" effect="slide" start="0,0" end="0,120"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(5)" effect="slide" start="0,0" end="0,160"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(6)" effect="slide" start="0,0" end="0,200"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(7)" effect="slide" start="0,0" end="0,240"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(8)" effect="slide" start="0,0" end="0,280"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(9)" effect="slide" start="0,0" end="0,320"
					time="60" reversible="true">Conditional</animation>
				<animation condition="Container(50).Position(10)" effect="slide" start="0,0" end="0,360"
					time="60" reversible="true">Conditional</animation>
			</control>
		</control>
		<control type="group">
			<visible>Control.IsVisible(50)</visible>
			<include>SortLetterNotification</include>
			<include>PageControl</include>
		</control>
	</include>
</includes>
