<includes>
	<include name="VideoWrapView">
		<control type="wraplist" id="54">
			<viewtype label="31943">Wraplist</viewtype>
			<posx>-28</posx>
			<posy>150</posy>
			<width>1000</width>
			<height>420</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>24</ondown>
			<include>list-fade</include>
			<focusposition>2</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>13</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>24</pagecontrol>
			<itemlayout height="292" width="50">
				<control type="image">
					<posx>-36</posx>
					<posy>269</posy>
					<height>103</height>
					<width>45</width>
					<texture>case-dvd.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-36</posx>
					<posy>272</posy>
					<height>96</height>
					<width>44</width>
					<texture diffuse="mask-rect.png">DefaultVideo.png</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-36</posx>
					<posy>272</posy>
					<height>96</height>
					<width>44</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$INFO[ListItem.Icon]</texture>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-32</posx>
					<posy>272</posy>
					<height>15</height>
					<width>15</width>
					<info>ListItem.Overlay</info>
					<colordiffuse>UnFocusedCovers</colordiffuse>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="405" width="138">
				<control type="image">
					<posx>-50</posx>
					<posy>80</posy>
					<height>322</height>
					<width>155</width>
					<texture diffuse="mask-rect.png">hi-lite.png</texture>
					<aspectratio>stretch</aspectratio>
					<colordiffuse>IconGlow</colordiffuse>
					<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-29</posx>
					<posy>115</posy>
					<height>257</height>
					<width>117</width>
					<texture>case-dvd.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>Skin.HasSetting(DVDcase) + !SubString(ListItem.Icon,Default,Left)</visible>
				</control>
				<control type="image">
					<posx>-30</posx>
					<posy>119</posy>
					<height>250</height>
					<width>115</width>
					<texture diffuse="mask-rect.png">DefaultVideo.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-30</posx>
					<posy>119</posy>
					<height>250</height>
					<width>115</width>
					<texture fallback="DefaultVideo.png" background="true" diffuse="mask-rect.png">$INFO[ListItem.Icon]</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!SubString(ListItem.Icon,DefaultFolder.png,Left)</visible>
				</control>
				<control type="image">
					<posx>-27</posx>
					<posy>124</posy>
					<height>39</height>
					<width>39</width>
					<info>ListItem.Overlay</info>
					<visible>Skin.HasSetting(watched-overlay)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<!-- Scroll Bar -->
			<control type="scrollbar" id="24">
				<posx>10</posx>
				<posy>527</posy>
				<width>700</width>
				<height>22</height>
				<texturesliderbackground border="11">scrollbar_background.png</texturesliderbackground>
				<texturesliderbar border="11">scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus border="11">scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<onup>54</onup>
				<ondown>2</ondown>
				<include>list-fade</include>
				<showonepage>false</showonepage>
				<orientation>horizontal</orientation>
				<colordiffuse>ScrollBarColor</colordiffuse>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<!-- Star Rating -->
			<control type="image">
				<posx>175</posx>
				<posy>275</posy>
				<width>100</width>
				<height>20</height>
				<info>ListItem.StarRating</info>
				<colordiffuse>StarDiffuse</colordiffuse>
				<visible>!SubString(ListItem.Icon,Default,Left)</visible>
				<visible>!IsEmpty(ListItem.StarRating)</visible>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- MPAA Rating -->
			<control type="group">
				<posx>175</posx>
				<posy>300</posy>
				<include>mpaa-ratings</include>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>175</posx>
				<posy>350</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Title -->
			<control type="label">
				<posx>175</posx>
				<posy>350</posy>
				<width>525</width>
				<height>30</height>
				<font>font22</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
			<!-- Genre -->
			<control type="label">
				<posx>175</posx>
				<posy>390</posy>
				<width>525</width>
				<height>30</height>
				<font>font13</font>
				<align>left</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<selectedcolor>listselectedtext</selectedcolor>
				<scroll>true</scroll>
				<info>ListItem.Genre</info>
				<textoffsetx>0</textoffsetx>
				<animation effect="fade" delay="300" start="0" end="100" time="200">Visible</animation>
			</control>
		</control>
	</include>
</includes>
