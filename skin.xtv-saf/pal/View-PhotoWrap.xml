<includes>
	<include name="PhotoWrap">
		<control type="wraplist" id="54">
			<viewtype label="31943">Wraplist</viewtype>
			<posx>-10</posx>
			<posy>155</posy>
			<width>1000</width>
			<height>400</height>
			<onleft>54</onleft>
			<onright>54</onright>
			<onup>2</onup>
			<ondown>23</ondown>
			<include>list-fade</include>
			<focusposition>1</focusposition>
			<orientation>horizontal</orientation>
			<preloaditems>6</preloaditems>
			<scrolltime>0</scrolltime>
			<pagecontrol>23</pagecontrol>
			<itemlayout height="191" width="108">
				<control type="image">
					<posx>65</posx>
					<posy>186</posy>
					<width>99</width>
					<height>144</height>
					<align>center</align>
					<fadetime>600</fadetime>
					<info>ListItem.Icon</info>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
				<control type="image">
					<posx>65</posx>
					<posy>186</posy>
					<width>99</width>
					<height>144</height>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<bordertexture border="6">picturepanel.png</bordertexture>
					<bordersize>6</bordersize>
					<fadetime>600</fadetime>
					<visible>!SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
			</itemlayout>
			<focusedlayout height="427" width="241">
				<control type="image">
					<posx>67</posx>
					<posy>0</posy>
					<height>330</height>
					<width>228</width>
					<texture background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<bordertexture border="6">picturepanel.png</bordertexture>
					<bordersize>6</bordersize>
					<fadetime>600</fadetime>
					<visible>!SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
				<control type="image">
					<posx>67</posx>
					<posy>0</posy>
					<height>330</height>
					<width>228</width>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<fadetime>600</fadetime>
					<info>ListItem.Icon</info>
					<visible>SubString(ListItem.Icon,Default,Left) + !SubString(ListItem.FileNameAndPath,/,Left)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="group">
			<visible>Control.IsVisible(54)</visible>
			<include>SortLetterNotification</include>
			<!-- Scroll Bar -->
			<control type="scrollbar" id="23">
				<posx>10</posx>
				<posy>490</posy>
				<width>700</width>
				<height>22</height>
				<texturesliderbackground border="10">scrollbar_background.png</texturesliderbackground>
				<texturesliderbar border="10">scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus border="10">scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<onup>54</onup>
				<ondown>2</ondown>
				<include>list-fade</include>
				<showonepage>false</showonepage>
				<orientation>horizontal</orientation>
				<colordiffuse>ScrollBarColor</colordiffuse>
				<visible>Skin.HasSetting(sbar)</visible>
			</control>
			<!-- Go Back -->
			<control type="label">
				<posx>10</posx>
				<posy>507</posy>
				<width>700</width>
				<height>30</height>
				<font>font22</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<label>31688</label>
				<textoffsetx>0</textoffsetx>
				<visible>SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Label -->
			<control type="label">
				<posx>10</posx>
				<posy>507</posy>
				<width>700</width>
				<height>30</height>
				<font>font22</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
				<textoffsetx>0</textoffsetx>
				<visible>!SubString(ListItem.Icon,DefaultFolderBack.png,Left)</visible>
			</control>
			<!-- Resolution -->
			<control type="label">
				<posx>160</posx>
				<posy>547</posy>
				<width>400</width>
				<height>30</height>
				<font>font13</font>
				<align>center</align>
				<textcolor>button-text</textcolor>
				<shadowcolor>shadow-text</shadowcolor>
				<scroll>true</scroll>
				<info>ListItem.PictureResolution</info>
				<textoffsetx>0</textoffsetx>
			</control>
		</control>
	</include>
</includes>
